            <xsd:documentation>Compact list of workflow "Activity History"</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attributeGroup ref="tcm:CommonListItemAttrs"/>
                        <xsd:attributeGroup ref="tcm:AllowedActionsListItemAttrs"/>
                        <xsd:attribute name="Type" type="xsd:int" use="required"/>
                        <xsd:attribute name="PublicationTitle" type="tcm:Title" use="optional"/>
                        <xsd:attribute name="TCMItemType" type="xsd:int" use="optional"/>
                        <xsd:attribute name="ActivityDefinitionTitle" type="tcm:Title" use="optional"/>
                        <xsd:attribute name="ActivityDefinitionType" type="xsd:string" use="optional"/>
                        <xsd:attribute name="ActivityDefinitionDescription" type="xsd:string" use="optional"/>
                        <xsd:attribute name="CreationDate" type="xsd:dateTime" use="optional"/>
                        <xsd:attribute name="StartDate" type="xsd:dateTime" use="optional"/>
                        <xsd:attribute name="FinishDate" type="xsd:dateTime" use="optional"/>
                        <xsd:attribute name="FinishMessage" type="xsd:string" use="optional"/>
                        <xsd:attribute name="HasSnapshot" type="xsd:boolean" use="optional"/>
                        <xsd:attribute name="Performer" type="xsd:string" use="optional"/>
                        <xsd:attribute name="Assignee" type="xsd:string" use="optional"/>
                        <xsd:attribute name="FinishCode" type="xsd:int" use="optional"/>
                        <xsd:attribute name="ApprovalStatus" type="tcm:Title" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListWFActivities">
        <xsd:annotation>
            <xsd:documentation>Compact list of Workflow Activities</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attributeGroup ref="tcm:CommonListItemAttrs"/>
                        <xsd:attributeGroup ref="tcm:AllowedActionsListItemAttrs"/>
                        <xsd:attribute name="Type" type="xsd:int" use="required"/>
                        <xsd:attribute name="PublicationTitle" type="tcm:Title" use="optional"/>
                        <xsd:attribute name="TCMItemType" type="xsd:int" use="optional"/>
                        <xsd:attribute name="ActivityDefinitionTitle" type="tcm:Title" use="optional"/>
                        <xsd:attribute name="ActivityDefinitionType" type="xsd:string" use="optional"/>
                        <xsd:attribute name="ActivityDefinitionDescription" type="xsd:string" use="optional"/>
                        <xsd:attribute name="CreationDate" type="xsd:dateTime" use="optional"/>
                        <xsd:attribute name="StartDate" type="xsd:dateTime" use="optional"/>
                        <xsd:attribute name="FinishDate" type="xsd:dateTime" use="optional"/>
                        <xsd:attribute name="FinishMessage" type="xsd:string" use="optional"/>
                        <xsd:attribute name="HasSnapshot" type="xsd:boolean" use="optional"/>
                        <xsd:attribute name="Performer" type="xsd:string" use="optional"/>
                        <xsd:attribute name="Assignee" type="xsd:string" use="optional"/>
                        <xsd:attribute name="FinishCode" type="xsd:int" use="optional"/>
                        <xsd:attribute name="ApprovalStatus" type="tcm:Title" use="optional"/>
                        <xsd:attribute name="DueDate" type="xsd:dateTime" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="ID" type="xsd:anyURI" use="required"/>
            <xsd:attribute name="ItemType" type="xsd:int" use="required"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListWFManagers">
        <xsd:annotation>
            <xsd:documentation>Compact list of workflow "Managers"</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attributeGroup ref="tcm:CommonListItemAttrs"/>
                        <xsd:attributeGroup ref="tcm:AllowedActionsListItemAttrs"/>
                        <xsd:attribute name="PublicationTitle" type="tcm:Title" use="required"/>
                        <xsd:attribute name="ProcessDefinitionTitle" type="tcm:Title" use="required"/>
                        <xsd:attribute name="ItemType" type="xsd:int" use="required"/>
                        <xsd:attribute name="WFItemType" type="xsd:int" use="required"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListWFParticipants">
        <xsd:annotation>
            <xsd:documentation>Compact list of workflow "Participants"</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attributeGroup ref="tcm:CommonListItemAttrs"/>
                        <xsd:attributeGroup ref="tcm:AllowedActionsListItemAttrs"/>
                        <xsd:attribute name="PublicationTitle" type="tcm:Title" use="required"/>
                        <xsd:attribute name="TCMItemType" type="xsd:int" use="required"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="WorkItem">
        <xsd:annotation>
            <xsd:documentation>Element of workflow "WorkItem"</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="tcm:Context" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>
                            Information in regards to the context of the item in Tridion Content Manager.                               
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="Info" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Data" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Read/write object properties</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="Title" type="tcm:Title" minOccurs="0"/>
                            <xsd:element name="ActivityInstance" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="TCMItem" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="Owner" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="Comment" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="SubjectOwningRepository" type="tcm:SimpleLink" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListWFWorkItems">
        <xsd:annotation>
            <xsd:documentation>Compact list of "Workflow Items"</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attributeGroup ref="tcm:CommonListItemAttrs"/>
                        <xsd:attributeGroup ref="tcm:AllowedActionsListItemAttrs"/>
                        <xsd:attribute name="PublicationTitle" type="tcm:Title" use="required"/>
                        <xsd:attribute name="TCMItem" type="xsd:anyURI" use="required"/>
                        <xsd:attribute name="TCMItemType" type="xsd:int" use="required"/>
                        <xsd:attribute name="ActivityDefinitionTitle" type="tcm:Title" use="required"/>
                        <xsd:attribute name="ActivityDefinitionType" type="xsd:int" use="required"/>
                        <xsd:attribute name="CreationDate" type="xsd:dateTime" use="required"/>
                        <xsd:attribute name="StartDate" type="xsd:dateTime" use="required"/>
                        <xsd:attribute name="State" type="xsd:string" use="required"/>
                        <xsd:attribute name="StateValue" type="xsd:int" use="required"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Snapshot">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Content">
                    <xsd:complexType>
                        <xsd:choice minOccurs="0">
                            <xsd:element ref="tcm:RenderedPage"/>
                            <xsd:element ref="tcm:RenderedComponentPresentation"/>
                            <xsd:element name="Script" type="xsd:string"/>
                        </xsd:choice>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Binaries" minOccurs="0">
                    <xsd:complexType>
                        <xsd:sequence maxOccurs="unbounded">
                            <xsd:element name="Binary">
                                <xsd:complexType>
                                    <xsd:attribute name="URL" type="xsd:string" use="required"/>
                                    <xsd:attribute name="Path" type="xsd:string" use="required"/>
                                    <xsd:attribute name="Id" type="xsd:int" use="required"/>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="ActivityId" type="xsd:anyURI" use="required"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListUsingItems">
        <xsd:annotation>
            <xsd:documentation>Compact list of using items (Where Used: "Used by")</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attributeGroup ref="tcm:CommonListItemAttrs"/>
                        <xsd:attributeGroup ref="tcm:AllowedActionsListItemAttrs"/>
                        <xsd:attribute name="Type" type="xsd:int" use="optional"/>
                        <xsd:attribute name="Path" type="xsd:string" use="optional"/>
                        <xsd:attribute name="Versions" use="optional">
                            <xsd:simpleType>
                                <xsd:list itemType="xsd:int"/>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="CommentToken" use="optional">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value="OnlyOldVersions"/>
                                    <xsd:enumeration value="OnlyDynamicVersion"/>
                                    <xsd:enumeration value="LocalCopy"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="Comment" type="xsd:string" use="optional"/>
                        <xsd:attribute name="OrgItemID" type="xsd:anyURI" use="optional"/>
                        <xsd:attribute name="SchemaId" type="xsd:anyURI" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListUsedItems">
        <xsd:annotation>
            <xsd:documentation>Compact list of used items (Where Used: "Uses")</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attributeGroup ref="tcm:CommonListItemAttrs"/>
                        <xsd:attributeGroup ref="tcm:AllowedActionsListItemAttrs"/>
                        <xsd:attribute name="Type" type="xsd:int" use="optional"/>
                        <xsd:attribute name="Path" type="xsd:string" use="optional"/>
                        <xsd:attribute name="SchemaId" type="xsd:anyURI" use="optional"/>
                        <xsd:attribute name="CommentToken" use="optional">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value="ParentItem"/>
                                    <xsd:enumeration value="ExternalLink"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="Comment" type="xsd:string" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListPublishItems">
        <xsd:annotation>
            <xsd:documentation>List of items that need to be (re-/un-)published</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="PublicationTarget" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Results are grouped per Publication Target</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:choice minOccurs="0" maxOccurs="unbounded">
                            <xsd:element name="Page">
                                <xsd:complexType>
                                    <xsd:attribute name="ID" type="xsd:anyURI" use="required"/>
                                    <xsd:attribute name="Title" type="xsd:string" use="optional"/>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Component">
                                <xsd:annotation>
                                    <xsd:documentation>For distinct Component Presentations</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="RenderWith" minOccurs="0" maxOccurs="unbounded">
                                            <xsd:annotation>
                                                <xsd:documentation>Component Template(s) to render the Component with (resulting in distinct Component Presentations)</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:attribute name="ID" type="xsd:anyURI" use="required"/>
                                                <xsd:attribute name="Title" type="xsd:string" use="optional"/>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                    <xsd:attribute name="ID" type="xsd:anyURI" use="required"/>
                                    <xsd:attribute name="Title" type="xsd:string" use="optional"/>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:choice>
                        <xsd:attribute name="ID" type="xsd:anyURI" use="required"/>
                        <xsd:attribute name="Title" type="xsd:string" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Error">
        <xsd:annotation>
            <xsd:documentation>Used as Description for errors raised by Tridion Content Manager</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Line" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Each (Re)RaiseError generates its own error Line. Error message is CDATA content.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType mixed="true">
                        <xsd:sequence>
                            <xsd:element name="Token" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>String resource tokens </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                        <xsd:attribute name="ErrorCode" type="xsd:string" use="optional"/>
                        <xsd:attribute name="Cause" type="xsd:boolean" use="optional" default="false"/>
                        <xsd:attribute name="MessageID" type="xsd:int" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Details">
                    <xsd:annotation>
                        <xsd:documentation>Contains detailed information about the error that occurred within Tridion Content Manager.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="CallStack">
                                <xsd:annotation>
                                    <xsd:documentation>Contains the call stack for the error that occurred within Tridion Content Manager</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="Location" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
                                            <xsd:annotation>
                                                <xsd:documentation>Contains the name of the method that (re)raised the error.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="ErrorCode" type="xsd:string" use="required"/>
            <xsd:attribute name="Category" type="xsd:int" use="optional"/>
            <xsd:attribute name="Source" type="xsd:string" use="optional"/>
            <xsd:attribute name="Severity" type="xsd:int" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="DirectoryService">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="ServerDnsName" type="xsd:string" minOccurs="0"/>
                <xsd:element name="TcpPort" type="xsd:positiveInteger" minOccurs="0"/>
                <xsd:element name="ImpersonationUser" type="xsd:string" minOccurs="0"/>
                <xsd:element name="SearchAccountDN" type="xsd:string" minOccurs="0"/>
                <xsd:element name="SearchPassword" type="xsd:string" minOccurs="0"/>
                <xsd:element name="UsernameAttribute" type="xsd:string" minOccurs="0"/>
                <xsd:element name="UserDescriptionAttribute" type="xsd:string" minOccurs="0"/>
                <xsd:element name="UsersBaseDN" type="xsd:string" minOccurs="0"/>
                <xsd:element name="GroupsBaseDN" type="xsd:string" minOccurs="0"/>
                <xsd:element name="GroupMemberAttribute" type="xsd:string" minOccurs="0"/>
                <xsd:element name="AdditionalAdsiFlags" type="xsd:unsignedLong" minOccurs="0"/>
                <xsd:element name="EncryptPassword" type="xsd:boolean" minOccurs="0"/>
            </xsd:sequence>
            <xsd:attribute name="Name" type="xsd:string" use="required"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LogFilterInfo">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Categories">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="Category" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Users">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="User" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Sources">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="Source" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="ServerNames">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="ServerName" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="SearchInstruction">
        <xsd:annotation>
            <xsd:documentation>Contains Parameters provided from GUI to TDSBL</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element name="GeneralParameters">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="SearchQuery" type="xsd:string"/>
                            <xsd:element name="SearchIn">
                                <xsd:complexType>
                                    <xsd:complexContent>
                                        <xsd:extension base="tcm:SimpleLink">
                                            <xsd:attribute name="Recursive" type="xsd:boolean" use="required"/>
                                        </xsd:extension>
                                    </xsd:complexContent>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="AdvancedParameters" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="Name" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:simpleContent>
                                        <xsd:extension base="tcm:NonEmptyString">
                                            <xsd:attribute name="caseSensitive" type="xsd:boolean" use="optional" default="false"/>
                                        </xsd:extension>
                                    </xsd:simpleContent>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Type" type="xsd:int" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Deprecated - Use ItemTypes instead</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ItemTypes" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="ItemType" type="xsd:int" maxOccurs="unbounded"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Author" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="Description" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:simpleContent>
                                        <xsd:extension base="xsd:string">
                                            <xsd:attribute name="caseSensitive" type="xsd:boolean" use="optional" default="false"/>
                                        </xsd:extension>
                                    </xsd:simpleContent>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Modified" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:choice minOccurs="0">
                                        <xsd:element name="BetweenDates">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="StartDate" type="xsd:dateTime" minOccurs="0"/>
                                                    <xsd:element name="EndDate" type="xsd:dateTime" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="LastMonths" type="xsd:int"/>
                                        <xsd:element name="LastDays" type="xsd:int"/>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Keyword" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="LockStatus" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence minOccurs="0">
                                        <xsd:element name="User" type="tcm:SimpleLink"/>
                                    </xsd:sequence>
                                    <xsd:attribute name="StatusType" type="xsd:int" use="required"/>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="BluePrinting" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence minOccurs="0">
                                        <xsd:element name="Publication" type="tcm:SimpleLink"/>
                                    </xsd:sequence>
                                    <xsd:attribute name="StatusType" type="xsd:string" use="required"/>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="WorkflowStatus" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="Process">
                                            <xsd:complexType>
                                                <xsd:complexContent>
                                                    <xsd:extension base="tcm:SimpleLink">
                                                        <xsd:sequence minOccurs="0">
                                                            <xsd:element name="Activity" type="tcm:SimpleLink"/>
                                                        </xsd:sequence>
                                                    </xsd:extension>
                                                </xsd:complexContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="NumberOfItems" type="xsd:int" minOccurs="0"/>
                            <xsd:element name="PublishState" type="xsd:nonNegativeInteger" minOccurs="0"/>
                            <xsd:element name="BasedOnSchema" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="Schema" maxOccurs="unbounded">
                                            <xsd:complexType>
                                                <xsd:complexContent>
                                                    <xsd:extension base="tcm:SimpleLink">
                                                        <xsd:sequence>
                                                            <xsd:element name="Element" minOccurs="0">
                                                                <xsd:complexType>
                                                                    <xsd:simpleContent>
                                                                        <xsd:extension base="xsd:string">
                                                                            <xsd:attribute name="baseName" type="xsd:string" use="required"/>
                                                                        </xsd:extension>
                                                                    </xsd:simpleContent>
                                                                </xsd:complexType>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:extension>
                                                </xsd:complexContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:complexType name="SimpleLinkError">
        <xsd:annotation>
            <xsd:documentation>"Extra error Info" for MultipleOperation</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tcm:SimpleLink">
                <xsd:attribute name="error" type="xsd:string" use="optional"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="SimpleLinkErrorVersion">
        <xsd:annotation>
            <xsd:documentation>"Extra Version Info" for DeleteVersion MultipleOperation</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tcm:SimpleLinkError">
                <xsd:attribute name="version" type="xsd:int" use="required"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="SimpleLinkErrorWarning">
        <xsd:annotation>
            <xsd:documentation>"Extra Version Info" for DeleteVersion MultipleOperation</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tcm:SimpleLinkError">
                <xsd:attribute name="warning" type="xsd:string" use="optional"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="MultipleOperations">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager MultipleOperations object</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Info" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Read-only info</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="VersionInfo" type="tcm:BasicVersionInfo" minOccurs="0"/>
                            <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Data" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Read/write object properties</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="Title" type="tcm:Title" minOccurs="0"/>
                            <xsd:element name="Operations" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                        <xsd:element name="Localize">
                                            <xsd:complexType>
                                                <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:element name="Component" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="ComponentTemplate" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Schema" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Page" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="PageTemplate" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="ProcessDefinition" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Folder" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="StructureGroup" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Category" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="TargetGroup" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="TemplateBuildingBlock" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Keyword" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="VirtualFolder" type="tcm:SimpleLinkError"/>
                                                </xsd:choice>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="UnLocalize">
                                            <xsd:complexType>
                                                <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:element name="Component" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="ComponentTemplate" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Schema" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Page" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="PageTemplate" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="ProcessDefinition" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Folder" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="StructureGroup" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Category" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="TargetGroup" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="TemplateBuildingBlock" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Keyword" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="VirtualFolder" type="tcm:SimpleLinkError"/>
                                                </xsd:choice>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="CheckIn">
                                            <xsd:complexType>
                                                <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:element name="Component" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="ComponentTemplate" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Schema" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Page" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="PageTemplate" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="TemplateBuildingBlock" type="tcm:SimpleLinkError"/>
                                                </xsd:choice>
                                                <xsd:attribute name="permanent" type="xsd:boolean" use="required"/>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="CheckOut">
                                            <xsd:complexType>
                                                <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:element name="Component" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="ComponentTemplate" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Schema" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Page" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="PageTemplate" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="TemplateBuildingBlock" type="tcm:SimpleLinkError"/>
                                                </xsd:choice>
                                                <xsd:attribute name="permanent" type="xsd:boolean" use="required"/>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="UndoCheckOut">
                                            <xsd:complexType>
                                                <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:element name="Component" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="ComponentTemplate" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Schema" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Page" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="PageTemplate" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="TemplateBuildingBlock" type="tcm:SimpleLinkError"/>
                                                </xsd:choice>
                                                <xsd:attribute name="permanent" type="xsd:boolean" use="required"/>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Delete">
                                            <xsd:complexType>
                                                <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:element name="Component" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="ComponentTemplate" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Schema" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Page" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="PageTemplate" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Folder" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="StructureGroup" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Category" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="TargetGroup" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="TemplateBuildingBlock" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Keyword" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="ApprovalStatus" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Group" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="User" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="MultimediaType" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="ProcessDefinition" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="Publication" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="PublicationTarget" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="ScheduleItem" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="TargetType" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="ProcessHistory" type="tcm:SimpleLinkError"/>
                                                    <xsd:element name="VirtualFolder" type="tcm:SimpleLinkError"/>
                                                </xsd:choice>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="DeleteKeywordBranch">
                                            <xsd:complexType>
                                                <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:element name="Keyword" type="tcm:SimpleLinkError"/>
                                                </xsd:choice>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="DeleteVersion">
                                            <xsd:complexType>
                                                <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:element name="Component" type="tcm:SimpleLinkErrorVersion"/>
                                                    <xsd:element name="ComponentTemplate" type="tcm:SimpleLinkErrorVersion"/>
                                                    <xsd:element name="Schema" type="tcm:SimpleLinkErrorVersion"/>
                                                    <xsd:element name="Page" type="tcm:SimpleLinkErrorVersion"/>
                                                    <xsd:element name="PageTemplate" type="tcm:SimpleLinkErrorVersion"/>
                                                    <xsd:element name="TemplateBuildingBlock" type="tcm:SimpleLinkErrorVersion"/>
                                                </xsd:choice>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="PasteItem">
                                            <xsd:complexType>
                                                <xsd:complexContent>
                                                    <xsd:extension base="tcm:SimpleLink">
                                                        <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                            <xsd:element name="Component" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="ComponentTemplate" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="Schema" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="Page" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="PageTemplate" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="Folder" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="StructureGroup" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="Keyword" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="TemplateBuildingBlock" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="TargetGroup" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="VirtualFolder" type="tcm:SimpleLinkError"/>
                                                        </xsd:choice>
                                                        <xsd:attribute name="action" use="required">
                                                            <xsd:simpleType>
                                                                <xsd:restriction base="xsd:string">
                                                                    <xsd:enumeration value="cut"/>
                                                                    <xsd:enumeration value="copy"/>
                                                                </xsd:restriction>
                                                            </xsd:simpleType>
                                                        </xsd:attribute>
                                                        <xsd:attribute name="makeUnique" type="xsd:boolean" use="required"/>
                                                    </xsd:extension>
                                                </xsd:complexContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Publish">
                                            <xsd:complexType>
                                                <xsd:complexContent>
                                                    <xsd:extension base="tcm:SimpleLink">
                                                        <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                            <xsd:element name="Component" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="ComponentTemplate" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="Page" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="PageTemplate" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="StructureGroup">
                                                                <xsd:complexType>
                                                                    <xsd:complexContent>
                                                                        <xsd:extension base="tcm:SimpleLinkError">
                                                                            <xsd:attribute name="onlyPublishedPages" type="xsd:boolean" use="optional" default="false"/>
                                                                        </xsd:extension>
                                                                    </xsd:complexContent>
                                                                </xsd:complexType>
                                                            </xsd:element>
                                                            <xsd:element name="Publication" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="Category" type="tcm:SimpleLinkError"/>
                                                        </xsd:choice>
                                                        <xsd:attribute name="locationWorkflow" type="xsd:boolean" use="required"/>
                                                        <xsd:attribute name="activateBlueprinting" type="xsd:boolean" use="required"/>
                                                        <xsd:attribute name="publishTime" type="xsd:dateTime" use="optional"/>
                                                        <xsd:attribute name="unpublishTime" type="xsd:dateTime" use="optional"/>
                                                        <xsd:attribute name="deployTime" type="xsd:dateTime" use="optional"/>
                                                        <xsd:attribute name="rollbackOnFailure" type="xsd:boolean" use="required"/>
                                                        <xsd:attribute name="priority" use="optional">
                                                            <xsd:simpleType>
                                                                <xsd:restriction base="xsd:int">
                                                                    <xsd:enumeration value="2"/>
                                                                    <xsd:enumeration value="4"/>
                                                                    <xsd:enumeration value="6"/>
                                                                </xsd:restriction>
                                                            </xsd:simpleType>
                                                        </xsd:attribute>
                                                        <xsd:attribute name="resolveComponentLinks" type="xsd:boolean" use="optional"/>
                                                        <xsd:attribute name="ignoreRenderFailures" type="xsd:boolean" use="optional"/>
                                                        <xsd:attribute name="maximumRenderFailures" type="xsd:positiveInteger" use="optional"/>
                                                    </xsd:extension>
                                                </xsd:complexContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="UnPublish">
                                            <xsd:complexType>
                                                <xsd:complexContent>
                                                    <xsd:extension base="tcm:SimpleLink">
                                                        <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                            <xsd:element name="Component" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="ComponentTemplate" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="Page" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="PageTemplate" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="StructureGroup" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="Publication" type="tcm:SimpleLinkError"/>
                                                            <xsd:element name="Category" type="tcm:SimpleLinkError"/>
                                                        </xsd:choice>
                                                        <xsd:attribute name="locationWorkflow" type="xsd:boolean" use="required"/>
                                                        <xsd:attribute name="activateBlueprinting" type="xsd:boolean" use="required"/>
                                                        <xsd:attribute name="unpublishTime" type="xsd:dateTime" use="optional"/>
                                                        <xsd:attribute name="rollbackOnFailure" type="xsd:boolean" use="required"/>
                                                        <xsd:attribute name="priority" use="optional">
                                                            <xsd:simpleType>
                                                                <xsd:restriction base="xsd:int">
                                                                    <xsd:enumeration value="2"/>
                                                                    <xsd:enumeration value="4"/>
                                                                    <xsd:enumeration value="6"/>
                                                                </xsd:restriction>
                                                            </xsd:simpleType>
                                                        </xsd:attribute>
                                                        <xsd:attribute name="resolveComponentLinks" type="xsd:boolean" use="optional"/>
                                                    </xsd:extension>
                                                </xsd:complexContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Enable">
                                            <xsd:complexType>
                                                <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:element name="User" type="tcm:SimpleLinkError"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Disable">
                                            <xsd:complexType>
                                                <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:element name="User" type="tcm:SimpleLinkError"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="StartActivity">
                                            <xsd:complexType>
                                                <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:element name="WorkItem" type="tcm:SimpleLinkError"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="FinishActivity">
                                            <xsd:complexType>
                                                <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:element name="WorkItem" type="tcm:SimpleLinkError"/>
                                                </xsd:sequence>
                                                <xsd:attribute name="message" type="xsd:string" use="required"/>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="FinishProcess">
                                            <xsd:complexType>
                                                <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:element name="ProcessInstance" type="tcm:SimpleLinkError"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Classify">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="KeywordsToAdd" maxOccurs="unbounded">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="Keyword" type="tcm:SimpleLink" maxOccurs="unbounded"/>
                                                            </xsd:sequence>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                        <xsd:element name="Component" type="tcm:SimpleLinkErrorWarning"/>
                                                        <xsd:element name="ComponentTemplate" type="tcm:SimpleLinkErrorWarning"/>
                                                        <xsd:element name="Page" type="tcm:SimpleLinkErrorWarning"/>
                                                        <xsd:element name="PageTemplate" type="tcm:SimpleLinkErrorWarning"/>
                                                        <xsd:element name="TemplateBuildingBlock" type="tcm:SimpleLinkErrorWarning"/>
                                                        <xsd:element name="Folder" type="tcm:SimpleLinkErrorWarning"/>
                                                        <xsd:element name="VirtualFolder" type="tcm:SimpleLinkErrorWarning"/>
                                                        <xsd:element name="StructureGroup" type="tcm:SimpleLinkErrorWarning"/>
                                                        <xsd:element name="Keyword" type="tcm:SimpleLinkErrorWarning"/>
                                                    </xsd:choice>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="UnClassify">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="KeywordsToRemove" maxOccurs="unbounded">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="Keyword" type="tcm:SimpleLink" maxOccurs="unbounded"/>
                                                            </xsd:sequence>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                        <xsd:element name="Component" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="ComponentTemplate" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="Page" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="PageTemplate" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="TemplateBuildingBlock" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="Folder" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="VirtualFolder" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="StructureGroup" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="Keyword" type="tcm:SimpleLinkError"/>
                                                    </xsd:choice>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="ReClassify">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="KeywordsToRemove" maxOccurs="unbounded">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="Keyword" type="tcm:SimpleLink" maxOccurs="unbounded"/>
                                                            </xsd:sequence>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="KeywordsToAdd" maxOccurs="unbounded">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="Keyword" type="tcm:SimpleLink" maxOccurs="unbounded"/>
                                                            </xsd:sequence>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                        <xsd:element name="Component" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="ComponentTemplate" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="Page" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="PageTemplate" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="TemplateBuildingBlock" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="Folder" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="VirtualFolder" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="StructureGroup" type="tcm:SimpleLinkError"/>
                                                        <xsd:element name="Keyword" type="tcm:SimpleLinkError"/>
                                                    </xsd:choice>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:choice>
                                    <xsd:attribute name="skipOnError" type="xsd:boolean" use="required"/>
                                    <xsd:attribute name="operationsCount" type="xsd:int" use="optional"/>
                                    <xsd:attribute name="operationsDone" type="xsd:int" use="optional"/>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Active" type="xsd:boolean" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="SchemaValidationResult">
        <xsd:annotation>
            <xsd:documentation>Result of a successfull Schema XML Validation</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element name="IsTridionWebSchema" type="xsd:boolean"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ComponentValidationResult">
        <xsd:annotation>
            <xsd:documentation>Result of a successfull Component XML Validation</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ListLocales">
        <xsd:annotation>
            <xsd:documentation>Compact list of installed "Locales"</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attribute name="ID" type="xsd:int"/>
                        <xsd:attribute name="Title" type="xsd:string"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListDeletedReferences">
        <xsd:annotation>
            <xsd:documentation>Result of GetListDeletedReferences</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attribute name="ID" type="xsd:anyURI" use="required"/>
                        <xsd:attribute name="Title" type="xsd:string" use="required"/>
                        <xsd:attribute name="Type" type="xsd:int" use="required"/>
                        <xsd:attribute name="SubType" type="xsd:int" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="RollbackComments">
        <xsd:annotation>
            <xsd:documentation>Result of GetRollbackComments</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attribute name="ID" type="xsd:anyURI" use="required"/>
                        <xsd:attribute name="Title" type="xsd:string" use="required"/>
                        <xsd:attribute name="Type" type="xsd:int" use="required"/>
                        <xsd:attribute name="Modified" type="xsd:dateTime" use="required"/>
                        <xsd:attribute name="SubType" type="xsd:int" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PasteItem">
        <xsd:annotation>
            <xsd:documentation>Result of a PastItem (or Clone)</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="NewURI" type="xsd:anyURI"/>
                <xsd:element name="NewTitle" type="tcm:Title"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListOrganizationalItems">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attributeGroup ref="tcm:CommonListItemAttrs"/>
                        <xsd:attributeGroup ref="tcm:AllowedActionsListItemAttrs"/>
                        <xsd:attributeGroup ref="tcm:ExtendedListItemAttrs"/>
                        <xsd:attribute name="Type" type="xsd:int" use="required"/>
                        <xsd:attribute name="Modified" type="xsd:dateTime" use="required"/>
                        <xsd:attribute name="FromPub" type="xsd:string" use="optional"/>
                        <xsd:attribute name="Size" type="xsd:int" use="optional"/>
                        <xsd:attribute name="URL" type="xsd:anyURI" use="optional"/>
                        <xsd:attribute name="MIMEType" type="xsd:string" use="optional"/>
                        <xsd:attribute name="ParentOrgItemID" type="xsd:anyURI" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="ID" type="xsd:anyURI" use="required"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListOrganizationalItemAncestors">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attributeGroup ref="tcm:CommonListItemAttrs"/>
                        <xsd:attributeGroup ref="tcm:AllowedActionsListItemAttrs"/>
                        <xsd:attribute name="PublishLocationPath" type="xsd:string" use="optional"/>
                        <xsd:attribute name="PublishLocationUrl" type="xsd:string" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="ID" type="xsd:anyURI" use="required"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListEnumeratedValues">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Enumeration" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="Name">
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value="PublishPriority"/>
                                        <xsd:enumeration value="PublishAction"/>
                                        <xsd:enumeration value="PublishTransactionState"/>
                                        <xsd:enumeration value="TDSServerSideLanguages"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="Value" minOccurs="0" maxOccurs="unbounded">
                                <xsd:complexType>
                                    <xsd:attribute name="ID" type="xsd:int" use="required"/>
                                    <xsd:attribute name="Title" type="xsd:string" use="required"/>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                        <xsd:attribute name="LocaleID" type="xsd:int" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="QueueMessage">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Data" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="Item" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="Action" minOccurs="0">
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:int">
                                        <xsd:enumeration value="0"/>
                                        <xsd:enumeration value="1"/>
                                        <xsd:enumeration value="2"/>
                                        <xsd:enumeration value="3"/>
                                        <xsd:enumeration value="4"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="Priority" minOccurs="0">
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:int">
                                        <xsd:enumeration value="2"/>
                                        <xsd:enumeration value="4"/>
                                        <xsd:enumeration value="6"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="Queue" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:attribute name="ID" type="xsd:int"/>
                                    <xsd:attribute name="name" type="xsd:string"/>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="PublicationTarget" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="ProcessingConsumer" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:attribute name="ID" type="xsd:int" use="optional"/>
                                    <xsd:attribute name="hostName" type="xsd:string" use="optional"/>
                                    <xsd:attribute name="port" type="xsd:int" use="optional"/>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="CreationHostname" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="ScheduleTime" type="xsd:dateTime" minOccurs="0"/>
                            <xsd:element name="QueueTime" type="xsd:dateTime" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="ID" type="xsd:long"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="QueueConsumer">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Data" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="QueueID" type="xsd:int" minOccurs="0"/>
                            <xsd:element name="Host" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Port" type="xsd:int" minOccurs="0"/>
                            <xsd:element name="IsOnline" type="xsd:boolean" minOccurs="0"/>
                            <xsd:element ref="tcm:QueueFilter" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="ID" type="xsd:int"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="QueueFilter">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Filter" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attribute name="name" type="xsd:string" use="required"/>
                        <xsd:attribute name="value" type="xsd:string" use="required"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListQueues">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attribute name="ID" type="xsd:int" use="required"/>
                        <xsd:attribute name="name" type="xsd:string" use="optional"/>
                        <xsd:attribute name="numberOfMessages" type="xsd:int" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListQueueConsumers">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element ref="tcm:QueueFilter" minOccurs="0"/>
                        </xsd:sequence>
                        <xsd:attribute name="ID" type="xsd:int" use="optional"/>
                        <xsd:attribute name="queueID" type="xsd:int" use="optional"/>
                        <xsd:attribute name="hostName" type="xsd:string" use="optional"/>
                        <xsd:attribute name="port" type="xsd:int" use="optional"/>
                        <xsd:attribute name="isOnLine" type="xsd:boolean" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListQueuedMessages">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attribute name="ID" type="xsd:int" use="required"/>
                        <xsd:attribute name="action" use="optional">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:int">
                                    <xsd:enumeration value="0"/>
                                    <xsd:enumeration value="1"/>
                                    <xsd:enumeration value="2"/>
                                    <xsd:enumeration value="3"/>
                                    <xsd:enumeration value="4"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="priority" use="optional">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:int">
                                    <xsd:enumeration value="2"/>
                                    <xsd:enumeration value="4"/>
                                    <xsd:enumeration value="6"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="queue" use="required">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:int">
                                    <xsd:enumeration value="1"/>
                                    <xsd:enumeration value="2"/>
                                    <xsd:enumeration value="3"/>
                                    <xsd:enumeration value="4"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="processingConsumerID" type="xsd:int" use="optional"/>
                        <xsd:attribute name="creationHostname" type="xsd:string" use="required"/>
                        <xsd:attribute name="queueTime" type="xsd:dateTime" use="required"/>
                        <xsd:attribute name="scheduleTime" type="xsd:dateTime" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="appinfo">
        <xsd:annotation>
            <xsd:documentation>AppInfo for Tridion Web Schemas</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="Labels" minOccurs="0">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="Label" minOccurs="0" maxOccurs="unbounded">
                                <xsd:complexType>
                                    <xsd:simpleContent>
                                        <xsd:extension base="xsd:string">
                                            <xsd:attribute name="ElementName" type="xsd:normalizedString" use="required"/>
                                            <xsd:attribute name="Metadata" type="xsd:string" use="required"/>
                                        </xsd:extension>
                                    </xsd:simpleContent>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="linktype" type="xsd:string" minOccurs="0"/>
                <xsd:element name="CustomURL" type="tcm:URIorEmpty" minOccurs="0"/>
                <xsd:element name="listtype" type="tcm:ListType" minOccurs="0"/>
                <xsd:element name="Size" type="xsd:int" minOccurs="0"/>
                <xsd:element name="FilterXSLT" type="tcm:XSLT" minOccurs="0"/>
                <xsd:element name="AllowedTargetSchemas" minOccurs="0">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="TargetSchema" type="tcm:SimpleLink" minOccurs="0" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="AllowMultimediaLinks" type="xsd:boolean" minOccurs="0"/>
                <xsd:element name="EmbeddedSchema" minOccurs="0">
                    <xsd:complexType>
                        <xsd:complexContent>
                            <xsd:extension base="tcm:SimpleLink">
                                <xsd:sequence>
                                    <xsd:element name="Fields" type="tcm:TridionWebSchemaFields" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:extension>
                        </xsd:complexContent>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="DefaultNumericValue" type="xsd:decimal" minOccurs="0"/>
                <xsd:element name="DefaultTextValue" type="xsd:normalizedString" minOccurs="0"/>
                <xsd:element name="DefaultMultiLineTextValue" type="tcmi:MultiLineText" minOccurs="0"/>
                <xsd:element name="DefaultFormattedTextValue" type="tcmi:XHTML" minOccurs="0"/>
                <xsd:element name="DefaultSimpleLinkValue" type="tcm:SimpleLink" minOccurs="0"/>
                <xsd:element name="DefaultDateTimeValue" type="xsd:dateTime" minOccurs="0"/>
                <xsd:element name="ExtensionXml" type="tcm:CustomMultiXML" minOccurs="0"/>
                <xsd:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:choice>
            <xsd:anyAttribute/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Taxonomies">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Taxonomy" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:any namespace="http://www.w3.org/1999/02/22-rdf-syntax-ns#" processContents="skip"/>
                        </xsd:sequence>
                        <xsd:attributeGroup ref="tcm:CommonListItemAttrs"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListTaxonomies">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attributeGroup ref="tcm:CommonListItemAttrs"/>
                        <xsd:attributeGroup ref="tcm:AllowedActionsListItemAttrs"/>
                        <xsd:attribute name="RootCategoryTitle" type="xsd:string" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:complexType name="ListPathToCategory_Type">
        <xsd:sequence>
            <xsd:element name="Category" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence minOccurs="0">
                        <xsd:element ref="tcm:ListKeywords" minOccurs="0"/>
                        <xsd:element name="Category" type="tcm:ListPathToCategory_Type" minOccurs="0"/>
                    </xsd:sequence>
                    <xsd:attributeGroup ref="tcm:CommonListItemAttrs"/>
                    <xsd:attributeGroup ref="tcm:AllowedActionsListItemAttrs"/>
                    <xsd:attribute name="Lock" type="xsd:int" use="optional"/>
                    <xsd:attribute name="Trustee" type="xsd:anyURI" use="optional"/>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="ListPathToCategory" type="tcm:ListPathToCategory_Type"/>
    <xsd:complexType name="Instructions_Type">
        <xsd:sequence>
            <xsd:element name="Local" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Instructions that can be applied to the rendered item</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:any namespace="##other" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Global" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Instructions that can be applied to all rendered items</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:any namespace="##other" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="ListContainingBundles">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attributeGroup ref="tcm:CommonListItemAttrs"/>
                        <xsd:attributeGroup ref="tcm:AllowedActionsListItemAttrs"/>
                        <xsd:attribute name="SchemaId" type="xsd:anyURI" use="optional"/>
                        <xsd:attribute name="Modified" type="xsd:dateTime" use="optional"/>
                        <xsd:attribute name="FromPub" type="xsd:string" use="optional"/>
                        <xsd:attribute name="IsLocalized" type="xsd:boolean" use="optional"/>
                        <xsd:attribute name="IsShared" type="xsd:boolean" use="optional"/>
                        <xsd:attribute name="LockType" type="xsd:int" use="optional"/>
                        <xsd:attribute name="LockUser" type="xsd:anyURI" use="optional"/>
                        <xsd:attribute name="Description" type="xsd:string" use="optional"/>
                        <xsd:attribute name="BundleTypeId" type="xsd:anyURI" use="optional"/>
                        <xsd:attribute name="BundleTypeTitle" type="xsd:string" use="optional"/>
                        <xsd:attribute name="BundleProcessId" type="xsd:anyURI" use="optional"/>
                        <xsd:attribute name="BundleProcessTitle" type="xsd:string" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ListWorkflowTypes">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attributeGroup ref="tcm:CommonListItemAttrs"/>
                        <xsd:attributeGroup ref="tcm:AllowedActionsListItemAttrs"/>
                        <xsd:attribute name="Name" type="xsd:string" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
