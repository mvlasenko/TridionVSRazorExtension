                                                            <xsd:element name="PerformingTimeout" type="xsd:int"/>
                                                            <xsd:element name="ApprovalStatus" type="tcm:SimpleLink" minOccurs="0"/>
                                                            <xsd:element name="NextActivityDefinitions">
