<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tcmi="http://www.tridion.com/ContentManager/5.0/Instance" xmlns:tcm="http://www.tridion.com/ContentManager/5.0" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:tcm6="http://www.sdltridion.com/ContentManager/R6" targetNamespace="http://www.tridion.com/ContentManager/5.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xsd:annotation>
        <xsd:documentation>This is the Tridion Content Manager system schema. This file should not be modified. For customizations, see cm_xml_usr.xsd</xsd:documentation>
    </xsd:annotation>
    <xsd:include schemaLocation="cm_xml_usr.xsd"/>
    <xsd:import namespace="http://www.w3.org/1999/xlink" schemaLocation="cm_lnk.xsd"/>
    <xsd:import namespace="http://www.tridion.com/ContentManager/5.0/Instance" schemaLocation="cm_xml_inst.xsd"/>
    <xsd:import namespace="http://www.sdltridion.com/ContentManager/R6" schemaLocation="cm_xml_publishing.xsd"/>
    <xsd:simpleType name="directorySearchType">
        <xsd:restriction base="xsd:positiveInteger">
            <xsd:enumeration value="1"/>
            <xsd:enumeration value="2"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="Occurrence">
        <xsd:annotation>
            <xsd:documentation>'unbounded' or Non-neg integer (for maxOccurs)</xsd:documentation>
        </xsd:annotation>
        <xsd:union memberTypes="xsd:nonNegativeInteger">
            <xsd:simpleType>
                <xsd:restriction base="xsd:NMTOKEN">
                    <xsd:enumeration value="unbounded"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:union>
    </xsd:simpleType>
    <xsd:simpleType name="ListType">
        <xsd:annotation>
            <xsd:documentation>Used in Tridion Web Schema</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="select"/>
            <xsd:enumeration value="radio"/>
            <xsd:enumeration value="checkbox"/>
            <xsd:enumeration value="edit"/>
            <xsd:enumeration value="tree"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="Base64orEmpty">
        <xsd:annotation>
            <xsd:documentation>xsd:base64Binary or an empty string</xsd:documentation>
        </xsd:annotation>
        <xsd:union memberTypes="xsd:base64Binary">
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:length value="0"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:union>
    </xsd:simpleType>
    <xsd:simpleType name="URIorEmpty">
        <xsd:annotation>
            <xsd:documentation>xsd:anyURI or an empty string</xsd:documentation>
        </xsd:annotation>
        <xsd:union memberTypes="xsd:anyURI">
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:length value="0"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:union>
    </xsd:simpleType>
    <xsd:simpleType name="NCNameOrEmpty">
        <xsd:annotation>
            <xsd:documentation>xsd:NCName or an empty string</xsd:documentation>
        </xsd:annotation>
        <xsd:union memberTypes="xsd:NCName">
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:length value="0"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:union>
    </xsd:simpleType>
    <xsd:simpleType name="DateTimeOrEmpty">
        <xsd:annotation>
            <xsd:documentation>xsd:dateTime or an empty string</xsd:documentation>
        </xsd:annotation>
        <xsd:union memberTypes="xsd:dateTime">
            <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:length value="0"/>
            </xsd:restriction>
            </xsd:simpleType>
        </xsd:union>
    </xsd:simpleType>
    <xsd:simpleType name="NonEmptyString">
        <xsd:annotation>
            <xsd:documentation>xsd:string that may not be empty</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="XSLT">
        <xsd:annotation>
            <xsd:documentation>Complex type used to allow XSLT content</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:any namespace="http://www.w3.org/1999/XSL/Transform" processContents="lax" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="XSD">
        <xsd:annotation>
            <xsd:documentation>Complex type used to allow XSD content</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:any namespace="http://www.w3.org/2001/XMLSchema" processContents="lax" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="CustomXML">
        <xsd:annotation>
            <xsd:documentation>Complex type used to allow custom XML content</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:any namespace="##other" processContents="lax" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="CustomMultiXML">
        <xsd:annotation>
            <xsd:documentation>Complex type used to allow mutliple custom XML content</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence maxOccurs="unbounded">
            <xsd:any namespace="##other" processContents="lax" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SimpleLink">
        <xsd:annotation>
            <xsd:documentation>Complex type used for relationships between items</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute ref="xlink:type" use="optional" fixed="simple"/>
        <xsd:attribute ref="xlink:title" use="optional"/>
        <xsd:attribute ref="xlink:href" use="required"/>
    </xsd:complexType>
    <xsd:attributeGroup name="ObjectAttrs">
        <xsd:annotation>
            <xsd:documentation>Attributes on each item element</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="ID" type="xsd:anyURI" use="optional"/>
        <xsd:attribute name="IsEditable" type="xsd:boolean" use="optional" default="true"/>
        <xsd:attribute name="IsDynamicVersion" type="xsd:boolean" use="optional" />
    </xsd:attributeGroup>
    <xsd:complexType name="BasicVersionInfo">
        <xsd:annotation>
            <xsd:documentation>"Version info" for organizational items</xsd:documentation>
        </xsd:annotation>
        <xsd:all>
            <xsd:element name="CreationDate" type="xsd:dateTime"/>
            <xsd:element name="RevisionDate" type="xsd:dateTime"/>
            <xsd:element name="Creator" type="tcm:SimpleLink" minOccurs="0"/>
        </xsd:all>
    </xsd:complexType>
    <xsd:complexType name="LimitedVersionInfo">
        <xsd:annotation>
            <xsd:documentation>Version info for organizational items</xsd:documentation>
        </xsd:annotation>
        <xsd:all>
            <xsd:element name="CreationDate" type="xsd:dateTime"/>
            <xsd:element name="RevisionDate" type="xsd:dateTime"/>
            <xsd:element name="Creator" type="tcm:SimpleLink"/>
        </xsd:all>
    </xsd:complexType>
    <xsd:complexType name="FullVersionInfo">
        <xsd:annotation>
            <xsd:documentation>Version info for versioned items</xsd:documentation>
        </xsd:annotation>
        <xsd:all>
            <xsd:element name="CreationDate" type="xsd:dateTime"/>
            <xsd:element name="RevisionDate" type="xsd:dateTime"/>
            <xsd:element name="Creator" type="tcm:SimpleLink"/>
            <xsd:element name="Version" type="xsd:integer"/>
            <xsd:element name="Revision" type="xsd:integer"/>
            <xsd:element name="Revisor" type="tcm:SimpleLink"/>
            <xsd:element name="ItemLock">
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element name="User" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="Date" type="xsd:dateTime" minOccurs="0"/>
                    </xsd:all>
                    <xsd:attribute name="Title" type="xsd:string" use="required"/>
                    <xsd:attribute name="Type" type="xsd:int" use="required"/>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="IsNew" type="xsd:boolean" minOccurs="0"/>
            <xsd:element name="SystemComment" type="xsd:string" minOccurs="0"/>
            <xsd:element name="UserComment" type="xsd:string" minOccurs="0"/>
        </xsd:all>
    </xsd:complexType>
  <xsd:complexType name="DynamicVersionInfo">
        <xsd:annotation>
            <xsd:documentation>Dynamic version info for versioned items</xsd:documentation>
        </xsd:annotation>
        <xsd:all>
          <xsd:element name="Revisor" type="tcm:SimpleLink"/>
          <xsd:element name="Revision" type="xsd:integer"/>
          <xsd:element name="RevisionDate" type="xsd:dateTime"/>
        </xsd:all>
    </xsd:complexType>
    <xsd:element name="LockInfo">
      <xsd:annotation>
        <xsd:documentation>Lock info for items that can be locked.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexType>
        <xsd:all>
          <xsd:element name="ItemLock">
            <xsd:complexType>
              <xsd:all>
                <xsd:element name="User" type="tcm:SimpleLink" minOccurs="0"/>
                <xsd:element name="Date" type="xsd:dateTime" minOccurs="0"/>
              </xsd:all>
              <xsd:attribute name="Title" type="xsd:string" use="required"/>
              <xsd:attribute name="Type" type="xsd:int" use="required"/>
            </xsd:complexType>
          </xsd:element>
        </xsd:all>
      </xsd:complexType>
    </xsd:element>
    <xsd:element name="WorkflowInfo">
        <xsd:annotation>
            <xsd:documentation>Workflow info for items that can participate in Workflow</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element name="ProcessInstance" type="tcm:SimpleLink"/>
                <xsd:element name="ActivityInstance">
                    <xsd:complexType>
                        <xsd:complexContent>
                            <xsd:extension base="tcm:SimpleLink">
                                <xsd:attribute name="Description" type="xsd:string" use="required"/>
                            </xsd:extension>
                        </xsd:complexContent>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Assignee" type="tcm:SimpleLink"/>
                <xsd:element name="Performer" type="tcm:SimpleLink"/>
                <xsd:element name="CreationDate" type="tcm:DateTimeOrEmpty"/>
                <xsd:element name="StartDate" type="tcm:DateTimeOrEmpty"/>
                <xsd:element name="FinishDate" type="tcm:DateTimeOrEmpty"/>
                <xsd:element name="AssignmentDate" type="tcm:DateTimeOrEmpty"/>
                <xsd:element name="DueDate" type="tcm:DateTimeOrEmpty"/>
                <xsd:element name="PreviousMessage" type="xsd:string"/>
                <xsd:element name="ActivityConstraints" type="xsd:string" minOccurs="0"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LocationInfo">
        <xsd:annotation>
            <xsd:documentation>Location information</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element name="WebDAVURL" type="xsd:anyURI" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The WebDAV URL of the item</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="Path" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>The plain path of the item, excluding the item's own title</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="PublishPath" type="xsd:string" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The publish path of a Page or SG, excluding the item's own filename</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="PublishLocationPath" type="xsd:string" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The publish path of a Page or Structure Group, including the item's own filename and extension</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="PublishLocationUrl" type="xsd:string" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The publish URL of a Page or Structure Group, including the item's own filename and extension</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemLink">
        <xsd:annotation>
            <xsd:documentation>Generic item link element</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tcm:SimpleLink"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="BluePrintInfo">
        <xsd:annotation>
            <xsd:documentation>BluePrinting information</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element name="OwningPublication" type="tcm:SimpleLink"/>
                <xsd:element name="IsShared" type="xsd:boolean"/>
                <xsd:element name="IsLocalized" type="xsd:boolean"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PublishInfo">
        <xsd:annotation>
            <xsd:documentation>Publishing information</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="IsPublished" type="xsd:boolean">
                    <xsd:annotation>
                        <xsd:documentation>Indicates if the item is published in the context Publication.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="PublishState" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Detailed publishing info. Only included if XMLReadPublishInfoDetails is specified.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="Publication" type="tcm:SimpleLink"/>
                            <xsd:element name="PublicationTarget" type="tcm:SimpleLink"/>
                            <xsd:element name="Date" type="xsd:dateTime"/>
                            <xsd:element name="Publisher" type="tcm:SimpleLink"/>
                            <xsd:element name="RenderWith" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="Item" type="tcm:SimpleLink"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:simpleType name="RenderMode">
        <xsd:annotation>
            <xsd:documentation>Used in publish and rendering</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Publish"/>
            <xsd:enumeration value="PreviewStatic"/>
            <xsd:enumeration value="PreviewDynamic"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:element name="RenderMode" type="tcm:RenderMode"/>
    <xsd:complexType name="CorePublishInfo">
        <xsd:sequence>
            <xsd:element ref="tcm:RenderMode"/>
        </xsd:sequence>
        <xsd:attribute name="BluePrintActive" type="xsd:boolean"/>
        <xsd:attribute name="WorkflowActive" type="xsd:boolean"/>
        <xsd:attribute ref="xlink:type" use="optional" fixed="simple"/>
        <xsd:attribute ref="xlink:href" use="required"/>
    </xsd:complexType>
    <xsd:element name="RenderInstruction">
        <xsd:complexType>
            <xsd:all>
                <xsd:element name="Item" type="tcm:SimpleLink"/>
                <xsd:element name="ActivateWorkflow" type="xsd:boolean"/>
                <xsd:element name="ActivateBlueprinting" type="xsd:boolean"/>
                <xsd:element ref="tcm:RenderMode"/>
                <xsd:element name="PublicationTarget" type="tcm:SimpleLink"/>
                <xsd:element name="BinaryLocation" type="xsd:string" minOccurs="0"/>
                <xsd:element name="RenderContext" minOccurs="0">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="ContextItem" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="ContextVariables" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="ContextVariable" minOccurs="0" maxOccurs="unbounded">
                                            <xsd:complexType>
                                                <xsd:attribute name="name" type="xsd:string" use="required"/>
                                                <xsd:attribute name="value" type="xsd:string" use="required"/>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:complexType name="ScheduleInstruction">
        <xsd:sequence>
            <xsd:element name="Executable" type="tcm:FilePath"/>
            <xsd:element name="ScheduleID" type="xsd:string"/>
            <xsd:element name="ScheduleDate" type="xsd:dateTime"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="ScheduleInstruction" type="tcm:ScheduleInstruction">
        <xsd:annotation>
            <xsd:documentation>Schedule instruction information</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="PublishInstruction_Type">
        <xsd:all>
            <xsd:element name="Item">
                <xsd:complexType>
                    <xsd:complexContent>
                        <xsd:extension base="tcm:SimpleLink">
                            <xsd:attribute name="itemType" type="xsd:int" use="optional"/>
                        </xsd:extension>
                    </xsd:complexContent>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="ActivateWorkflow" type="xsd:boolean"/>
            <xsd:element name="ActivateBlueprinting" type="xsd:boolean"/>
            <xsd:element name="RollbackOnFailure" type="xsd:boolean"/>
            <xsd:element name="Targets">
                <xsd:complexType>
                    <xsd:choice maxOccurs="unbounded">
                        <xsd:element name="PublicationTarget" type="tcm:SimpleLink"/>
                        <xsd:element name="TargetType" type="tcm:SimpleLink"/>
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <xsd:element ref="tcm:RenderMode" minOccurs="0"/>
            <xsd:element name="PublishTime" type="xsd:dateTime" minOccurs="0"/>
            <xsd:element name="UnpublishTime" type="xsd:dateTime" minOccurs="0"/>
            <xsd:element name="DeployTime" type="xsd:dateTime" minOccurs="0"/>
            <xsd:element name="OnlyPublishedItems" type="xsd:boolean" minOccurs="0"/>
            <xsd:element name="ResolveComponentLinks" type="xsd:boolean" minOccurs="0"/>
            <xsd:element name="Priority" minOccurs="0">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="2"/>
                        <xsd:enumeration value="4"/>
                        <xsd:enumeration value="6"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="IgnoreRenderFailures" minOccurs="0">
                <xsd:complexType>
                    <xsd:simpleContent>
                        <xsd:extension base="xsd:boolean">
                            <xsd:attribute name="limit" type="xsd:positiveInteger" use="optional"/>
                        </xsd:extension>
                    </xsd:simpleContent>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Instruction" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:any namespace="##any" processContents="lax"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>
    <xsd:element name="PublishInstruction" type="tcm:PublishInstruction_Type"/>
    <xsd:element name="PublishTransaction">
        <xsd:complexType>
            <xsd:all>
                <xsd:element name="Data" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="Action" minOccurs="0">
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:int">
                                        <xsd:enumeration value="0"/>
                                        <xsd:enumeration value="1"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="Item" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:complexContent>
                                        <xsd:extension base="tcm:SimpleLink">
                                            <xsd:attribute name="itemType" type="xsd:int" use="optional"/>
                                        </xsd:extension>
                                    </xsd:complexContent>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Publication" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:complexContent>
                                        <xsd:extension base="tcm:SimpleLink"/>
                                    </xsd:complexContent>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="PublicationTarget" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="Publisher" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:complexContent>
                                        <xsd:extension base="tcm:SimpleLink">
                                            <xsd:attribute name="description" type="xsd:string" use="optional"/>
                                        </xsd:extension>
                                    </xsd:complexContent>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="State" minOccurs="0">
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:int">
                                        <xsd:enumeration value="0"/>
                                        <xsd:enumeration value="1"/>
                                        <xsd:enumeration value="2"/>
                                        <xsd:enumeration value="3"/>
                                        <xsd:enumeration value="4"/>
                                        <xsd:enumeration value="5"/>
                                        <xsd:enumeration value="6"/>
                                        <xsd:enumeration value="7"/>
                                        <xsd:enumeration value="8"/>
                                        <xsd:enumeration value="9"/>
                                        <xsd:enumeration value="10"/>
                                        <xsd:enumeration value="11"/>
                                        <xsd:enumeration value="12"/>
                                        <xsd:enumeration value="13"/>
                                        <xsd:enumeration value="14"/>
                                        <xsd:enumeration value="15"/>
                                        <xsd:enumeration value="16"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="StateChangeDate" type="xsd:dateTime" minOccurs="0"/>
                            <xsd:element name="Priority" minOccurs="0">
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:int">
                                        <xsd:enumeration value="2"/>
                                        <xsd:enumeration value="4"/>
                                        <xsd:enumeration value="6"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="PublishTime" type="xsd:dateTime" minOccurs="0"/>
                            <xsd:element name="DeployTime" type="xsd:dateTime" minOccurs="0"/>
                            <xsd:element name="Information" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="IgnoreRenderFailures" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:simpleContent>
                                        <xsd:extension base="xsd:boolean">
                                            <xsd:attribute name="limit" type="xsd:positiveInteger" use="optional"/>
                                        </xsd:extension>
                                    </xsd:simpleContent>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element ref="tcm:PublisherRequest" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
            <xsd:attribute name="ID" type="xsd:anyURI" use="optional"/>
            <xsd:attribute name="IsEditable" type="xsd:boolean" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:complexType name="RenderedPage_Type">
        <xsd:complexContent>
            <xsd:extension base="tcm:Page_Type">
                <xsd:sequence>
                    <xsd:element name="RenderedContent">
                        <xsd:complexType>
                            <xsd:attribute name="CodePage" type="xsd:int" use="required"/>
                            <xsd:attribute name="IsRenderedCompletely" type="xsd:boolean" use="optional"/>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="RenderedComponentPresentations">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="RenderedComponentPresentation" type="tcm:RenderedComponentPresentation_Type"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="RenderedMultiMedias">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="RenderedMultiMedia" type="tcm:RenderedMultiMedia" minOccurs="0" maxOccurs="unbounded"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element ref="tcm:PageTemplate"/>
                    <xsd:element ref="tcm:Schema" minOccurs="0"/>
                    <xsd:element name="Metadata" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Metadata for the rendered Page </xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:any namespace="##other" maxOccurs="unbounded"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Instructions" type="tcm:Instructions_Type" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Instructions that can be handled by the Content Delivery subsystem</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="RenderedPage" type="tcm:RenderedPage_Type"/>
    <xsd:complexType name="RenderedComponentPresentation_Type">
        <xsd:sequence>
            <xsd:element name="RenderedContent">
                <xsd:complexType>
                    <xsd:simpleContent>
                        <xsd:extension base="xsd:string">
                            <xsd:attribute name="CodePage" type="xsd:int" use="optional"/>
                            <xsd:attribute name="IsRenderedCompletely" type="xsd:boolean" use="optional"/>
                        </xsd:extension>
                    </xsd:simpleContent>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Component" type="tcm:Component_Type"/>
            <xsd:element name="ComponentTemplate" type="tcm:ComponentTemplate_Type"/>
            <xsd:element name="Schema" type="tcm:Schema_Type"/>
            <xsd:element name="RenderedMultiMedias">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="RenderedMultiMedia" type="tcm:RenderedMultiMedia" minOccurs="0" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Metadata" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Metadata for the rendered Component Presentation</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:any namespace="##other" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Instructions" type="tcm:Instructions_Type" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Instructions that can be handled by the Content Delivery subsystem</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="IsRendered" type="xsd:boolean" use="required"/>
    </xsd:complexType>
    <xsd:element name="RenderedComponentPresentation" type="tcm:RenderedComponentPresentation_Type"/>
    <xsd:complexType name="RenderedMultiMedia">
        <xsd:sequence>
            <xsd:element name="Component" minOccurs="0">
                <xsd:complexType>
                    <xsd:complexContent>
                        <xsd:extension base="tcm:Component_Type">
                            <xsd:sequence>
                                <xsd:element name="TempFilename" type="xsd:string"/>
                                <xsd:element name="DeployedFilename" type="xsd:string"/>
                                <xsd:element name="Filename" type="xsd:string"/>
                                <xsd:element name="VariantId" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="MIMEType" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="RelatedComponent" type="tcm:SimpleLink" minOccurs="0"/>
                                <xsd:element name="Location" type="tcm:SimpleLink" minOccurs="0"/>
                            </xsd:sequence>
                        </xsd:extension>
                    </xsd:complexContent>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="TempFilename" type="xsd:string" minOccurs="0"/>
            <xsd:element name="DeployedFilename" type="xsd:string" minOccurs="0"/>
            <xsd:element name="Filename" type="xsd:string" minOccurs="0"/>
            <xsd:element name="VariantId" type="xsd:string" minOccurs="0"/>
            <xsd:element name="MIMEType" type="xsd:string" minOccurs="0"/>
            <xsd:element name="RelatedComponent" type="tcm:SimpleLink" minOccurs="0"/>
            <xsd:element name="Location" type="tcm:SimpleLink" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="PublisherRequest" type="tcm:PublisherRequest_Type">
        <xsd:annotation>
            <xsd:documentation>This is the root element of any publisher request.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="PublisherRequest_Type">
        <xsd:all>
            <xsd:element name="RollbackOnFailure" type="xsd:boolean" minOccurs="0"/>
            <xsd:element name="DeployerRequest" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="Action" use="required">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value="Abort"/>
                                <xsd:enumeration value="Commit"/>
                                <xsd:enumeration value="Wait"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:attribute>
                    <xsd:attribute name="DeploymentTime" type="xsd:dateTime" use="optional"/>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="OnlyPublishedItems" type="xsd:boolean" minOccurs="0"/>
            <xsd:element name="ActivateWorkflow" type="xsd:boolean" minOccurs="0"/>
            <xsd:element name="ResolveComponentLinks" type="xsd:boolean" minOccurs="0"/>
            <xsd:element ref="tcm:RenderInstruction" minOccurs="0"/>
            <xsd:element name="Schemas" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Schema" type="tcm:SimpleLink" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="ProcessedItems" minOccurs="0">
                <xsd:complexType>
                    <xsd:choice minOccurs="0" maxOccurs="unbounded">
                        <xsd:element name="Page">
                            <xsd:complexType>
                                <xsd:all>
                                    <xsd:element name="ReasonOfRenderFailure" type="xsd:string" minOccurs="0"/>
                                </xsd:all>
                                <xsd:attribute name="ID" type="xsd:anyURI" use="required"/>
                                <xsd:attribute name="RenderTime" type="xsd:string" use="optional"/>
                                <xsd:attribute name="IsRendered" type="xsd:boolean" use="optional"/>
                                <xsd:attribute name="hasRenderFailure" type="xsd:boolean" use="optional"/>
                                <xsd:attribute name="title" type="xsd:string" use="optional"/>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ComponentPresentation">
                            <xsd:complexType>
                                <xsd:all>
                                    <xsd:element name="ReasonOfRenderFailure" type="xsd:string" minOccurs="0"/>
                                </xsd:all>
                                <xsd:attribute name="ComponentID" type="xsd:anyURI" use="required"/>
                                <xsd:attribute name="ComponentTemplateID" type="xsd:anyURI" use="required"/>
                                <xsd:attribute name="RenderTime" type="xsd:string" use="optional"/>
                                <xsd:attribute name="IsRendered" type="xsd:boolean" use="optional"/>
                                <xsd:attribute name="hasRenderFailure" type="xsd:boolean" use="optional"/>
                                <xsd:attribute name="componentTitle" type="xsd:string" use="optional"/>
                                <xsd:attribute name="componentTemplateTitle" type="xsd:string" use="optional"/>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ProcessedItem">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="ResolvedItem">
                                        <xsd:complexType>
                                            <xsd:all>
                                                <xsd:element name="Item" type="tcm:SimpleLink"/>
                                                <xsd:element name="Template" type="tcm:SimpleLink" minOccurs="0"/>
                                            </xsd:all>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="ReasonOfRenderFailure" type="xsd:string" minOccurs="0"/>
                                </xsd:sequence>
                                <xsd:attribute name="renderTime" type="xsd:string" use="optional"/>
                                <xsd:attribute name="hasRenderFailure" type="xsd:boolean" use="optional"/>
                                <xsd:attribute name="isRendered" type="xsd:boolean" use="optional"/>
                                <xsd:attribute name="undoState" type="xsd:string" use="optional"/>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Instruction" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:any namespace="##any" processContents="skip"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:all>
        <xsd:attribute name="TransactionNumber" type="xsd:string" use="optional"/>
    </xsd:complexType>
    <xsd:element name="PublishResponse" type="tcm:PublishResponse_Type">
        <xsd:annotation>
            <xsd:documentation>Publish response, returned from the .Publish message containg the send publish messages or schedule acrtion</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="PublishResponse_Type">
        <xsd:sequence>
            <xsd:element name="PublisherRequest" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element name="Instruction">
                            <xsd:complexType>
                                <xsd:attribute name="Type" use="required">
                                    <xsd:simpleType>
                                        <xsd:restriction base="xsd:long">
                                            <xsd:enumeration value="1"/>
                                            <xsd:enumeration value="2"/>
                                        </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:attribute>
                                <xsd:attribute name="Title" type="xsd:normalizedString" use="required"/>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="Item" type="tcm:SimpleLink"/>
                        <xsd:element name="Publication" type="tcm:SimpleLink"/>
                        <xsd:element name="PublicationTarget" type="tcm:SimpleLink"/>
                        <xsd:element name="RollbackOnFailure" type="xsd:boolean"/>
                        <xsd:element name="User" type="tcm:SimpleLink"/>
                        <xsd:element name="DeployerRequest">
                            <xsd:complexType>
                                <xsd:attribute name="Action" use="required">
                                    <xsd:simpleType>
                                        <xsd:restriction base="xsd:string">
                                            <xsd:enumeration value="Now"/>
                                            <xsd:enumeration value="Wait"/>
                                            <xsd:enumeration value="Cancel"/>
                                        </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:attribute>
                                <xsd:attribute name="DeploymentTime" type="xsd:dateTime" use="optional"/>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="OnlyPublishedItems" type="xsd:boolean" minOccurs="0"/>
                        <xsd:element name="ActivateWorkflow" type="xsd:boolean" minOccurs="0"/>
                        <xsd:element name="ResolveComponentLinks" type="xsd:boolean" minOccurs="0"/>
                        <xsd:element ref="tcm:RenderInstruction" minOccurs="0"/>
                        <xsd:element name="Schemas" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="Schema" type="tcm:SimpleLink" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ProcessedItems" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="Page" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:complexType>
                                            <xsd:attribute name="ID" type="xsd:anyURI" use="required"/>
                                            <xsd:attribute name="RenderTime" type="xsd:string" use="optional"/>
                                            <xsd:attribute name="IsRendered" type="xsd:boolean" use="optional"/>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="ComponentPresentation" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:complexType>
                                            <xsd:attribute name="ComponentID" type="xsd:anyURI" use="required"/>
                                            <xsd:attribute name="ComponentTemplateID" type="xsd:anyURI" use="required"/>
                                            <xsd:attribute name="RenderTime" type="xsd:string" use="optional"/>
                                            <xsd:attribute name="IsRendered" type="xsd:boolean" use="optional"/>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:all>
                    <xsd:attribute name="TransactionNumber" type="xsd:string" use="optional"/>
                    <xsd:attribute name="PublishTransactionID" type="xsd:anyURI" use="optional"/>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="PublishScheduled" minOccurs="0">
                <xsd:complexType>
                    <xsd:complexContent>
                        <xsd:extension base="tcm:PublishInstruction_Type">
                            <xsd:attribute name="PublishTransactionID" type="xsd:anyURI" use="optional"/>
                        </xsd:extension>
                    </xsd:complexContent>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="RenderResponse_Type">
        <xsd:choice>
            <xsd:element name="RenderedPage" type="tcm:RenderedPage_Type"/>
            <xsd:element name="RenderedComponentPresentation" type="tcm:RenderedComponentPresentation_Type"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:element name="Context">
        <xsd:annotation>
            <xsd:documentation>Provides context info for an item.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element name="Publication" type="tcm:SimpleLink" minOccurs="0"/>
                <xsd:element name="OrganizationalItem" type="tcm:SimpleLink"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Permissions">
        <xsd:annotation>
            <xsd:documentation>Permissions settings for the Organizational Item</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="PermissionInfo" minOccurs="0">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="PermissionType" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Permissions Type that can be set on a Trustee</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="Title" type="xsd:string" use="required"/>
                                    <xsd:attribute name="ID" use="required">
                                        <xsd:simpleType>
                                            <xsd:restriction base="xsd:int">
                                                <xsd:enumeration value="1"/>
                                                <xsd:enumeration value="2"/>
                                                <xsd:enumeration value="4"/>
                                                <xsd:enumeration value="8"/>
                                                <xsd:enumeration value="16"/>
                                                <xsd:enumeration value="32"/>
                                                <xsd:enumeration value="64"/>
                                                <xsd:enumeration value="128"/>
                                                <xsd:enumeration value="256"/>
                                                <xsd:enumeration value="512"/>
                                                <xsd:enumeration value="1024"/>
                                                <xsd:enumeration value="2048"/>
                                                <xsd:enumeration value="4096"/>
                                                <xsd:enumeration value="8192"/>
                                                <xsd:enumeration value="16384"/>
                                                <xsd:enumeration value="65536"/>
                                            </xsd:restriction>
                                        </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute name="DependsOn" type="xsd:int" use="optional">
                                        <xsd:annotation>
                                            <xsd:documentation>Used to convey the dependencies between the permissions. e.g. if a trustee has Write, they must have read. Write will therefore have a DependsOn attribute referring to Read. Such dependancies are transitive.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Trustee" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:complexContent>
                            <xsd:extension base="tcm:SimpleLink">
                                <xsd:sequence>
                                    <xsd:element name="Permission" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:complexType>
                                            <xsd:attribute name="Type" use="required">
                                                <xsd:simpleType>
                                                    <xsd:restriction base="xsd:int">
                                                        <xsd:enumeration value="1"/>
                                                        <xsd:enumeration value="2"/>
                                                        <xsd:enumeration value="4"/>
                                                        <xsd:enumeration value="8"/>
                                                        <xsd:enumeration value="16"/>
                                                        <xsd:enumeration value="32"/>
                                                        <xsd:enumeration value="64"/>
                                                        <xsd:enumeration value="128"/>
                                                        <xsd:enumeration value="256"/>
                                                        <xsd:enumeration value="512"/>
                                                        <xsd:enumeration value="1024"/>
                                                        <xsd:enumeration value="2048"/>
                                                        <xsd:enumeration value="4096"/>
                                                        <xsd:enumeration value="8192"/>
                                                        <xsd:enumeration value="16384"/>
                                                        <xsd:enumeration value="65536"/>
                                                    </xsd:restriction>
                                                </xsd:simpleType>
                                            </xsd:attribute>
                                            <xsd:attribute name="Setting" use="required">
                                                <xsd:simpleType>
                                                    <xsd:restriction base="xsd:string">
                                                        <xsd:enumeration value="Allow"/>
                                                        <xsd:enumeration value="Deny"/>
                                                        <xsd:enumeration value="Implicit Deny"/>
                                                    </xsd:restriction>
                                                </xsd:simpleType>
                                            </xsd:attribute>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                                <xsd:attribute name="ItemType" type="xsd:int" use="optional"/>
                            </xsd:extension>
                        </xsd:complexContent>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="IsInheritanceRoot" type="xsd:boolean" use="optional" default="false"/>
            <xsd:attribute name="MayInherit" type="xsd:boolean" use="optional" default="true"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Rights">
        <xsd:annotation>
            <xsd:documentation>User Rights settings for a publication. </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="RightInfo" minOccurs="0">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="RightType" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Permissions Type that can be set on a Trustee</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="Title" type="xsd:string" use="required"/>
                                    <xsd:attribute name="ID" use="required">
                                        <xsd:simpleType>
                                            <xsd:restriction base="xsd:int">
                                                <xsd:enumeration value="1"/>
                                                <xsd:enumeration value="2"/>
                                                <xsd:enumeration value="4"/>
                                                <xsd:enumeration value="8"/>
                                                <xsd:enumeration value="16"/>
                                                <xsd:enumeration value="32"/>
                                                <xsd:enumeration value="64"/>
                                                <xsd:enumeration value="128"/>
                                                <xsd:enumeration value="256"/>
                                                <xsd:enumeration value="512"/>
                                                <xsd:enumeration value="1024"/>
                                                <xsd:enumeration value="2048"/>
                                                <xsd:enumeration value="4096"/>
                                                <xsd:enumeration value="8192"/>
                                                <xsd:enumeration value="16384"/>
                                                <xsd:enumeration value="32768"/>
                                                <xsd:enumeration value="65536"/>
                                            </xsd:restriction>
                                        </xsd:simpleType>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Trustee" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:complexContent>
                            <xsd:extension base="tcm:SimpleLink">
                                <xsd:sequence>
                                    <xsd:element name="Right" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:complexType>
                                            <xsd:attribute name="Type" use="required">
                                                <xsd:simpleType>
                                                    <xsd:restriction base="xsd:int">
                                                        <xsd:enumeration value="1"/>
                                                        <xsd:enumeration value="2"/>
                                                        <xsd:enumeration value="4"/>
                                                        <xsd:enumeration value="8"/>
                                                        <xsd:enumeration value="16"/>
                                                        <xsd:enumeration value="32"/>
                                                        <xsd:enumeration value="64"/>
                                                        <xsd:enumeration value="128"/>
                                                        <xsd:enumeration value="256"/>
                                                        <xsd:enumeration value="512"/>
                                                        <xsd:enumeration value="1024"/>
                                                        <xsd:enumeration value="2048"/>
                                                        <xsd:enumeration value="4096"/>
                                                        <xsd:enumeration value="8192"/>
                                                        <xsd:enumeration value="16384"/>
                                                        <xsd:enumeration value="32768"/>
                                                        <xsd:enumeration value="65536"/>
                                                    </xsd:restriction>
                                                </xsd:simpleType>
                                            </xsd:attribute>
                                            <xsd:attribute name="Setting" use="required">
                                                <xsd:simpleType>
                                                    <xsd:restriction base="xsd:string">
                                                        <xsd:enumeration value="Allow"/>
                                                        <xsd:enumeration value="Deny"/>
                                                        <xsd:enumeration value="Implicit Deny"/>
                                                    </xsd:restriction>
                                                </xsd:simpleType>
                                            </xsd:attribute>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                                <xsd:attribute name="ItemType" type="xsd:int" use="optional"/>
                            </xsd:extension>
                        </xsd:complexContent>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AllowedActions">
        <xsd:annotation>
            <xsd:documentation>Actions allowed on an Item</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element name="Actions" minOccurs="0">
                    <xsd:complexType>
                        <xsd:attribute name="Allow" type="xsd:int" use="required"/>
                        <xsd:attribute name="Deny" type="xsd:int" use="required"/>
                        <xsd:attribute name="Managed" type="xsd:int" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="GroupMemberships">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Group" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:complexContent>
                            <xsd:extension base="tcm:SimpleLink">
                                <xsd:sequence>
                                    <xsd:element name="MembershipConstraint" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:complexType>
                                            <xsd:complexContent>
                                                <xsd:extension base="tcm:SimpleLink">
                                                    <xsd:attribute name="IsEditable" type="xsd:boolean" use="optional" default="true"/>
                                                </xsd:extension>
                                            </xsd:complexContent>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                                <xsd:attribute name="IsEditable" type="xsd:boolean" use="optional" default="true"/>
                            </xsd:extension>
                        </xsd:complexContent>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Publication">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager Publication object</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Info" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Read-only object info</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element ref="tcm:LocationInfo" minOccurs="0"/>
                            <xsd:element name="VersionInfo" type="tcm:BasicVersionInfo" minOccurs="0"/>
                            <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                            <xsd:element name="RootFolder" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="RootStructureGroup" type="tcm:SimpleLink" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Data" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Read/write object properties</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType mixed="false">
                        <xsd:all>
                            <xsd:element name="Title" type="tcm:OrgItemTitle" minOccurs="0"/>
                            <xsd:element name="PublicationPath" type="tcm:FilePath" minOccurs="0"/>
                            <xsd:element name="PublicationURL" type="xsd:anyURI" minOccurs="0"/>
                            <xsd:element name="MultimediaPath" type="tcm:FilePath" minOccurs="0"/>
                            <xsd:element name="MultimediaURL" type="xsd:anyURI" minOccurs="0"/>
                            <xsd:element name="DefaultPageTemplate" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="DefaultComponentTemplate" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="DefaultTemplateBuildingBlock" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="DefaultMultimediaSchema" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="ParentPublications" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="ParentPublication" type="tcm:SimpleLink" minOccurs="0" maxOccurs="unbounded"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="PageTemplateProcess" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="ComponentTemplateProcess" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="TaskProcess" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="TemplateBundleProcess" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="ShareProcessAssociations" type="xsd:boolean" minOccurs="0"/>
                            <xsd:element name="ComponentSnapshotTemplate" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="PageSnapshotTemplate" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="MetadataSchema" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="Metadata" type="tcm:CustomXML" minOccurs="0"/>
                            <xsd:element name="CategoriesXSD" type="tcm:XSD" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Contains the XSD describing all Categories and Keywords in a publication.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="PublicationType" type="xsd:normalizedString" minOccurs="0"/>
                            <xsd:element name="Key" minOccurs="0">
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:normalizedString">
                                        <xsd:maxLength value="255"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Items" minOccurs="0">
                    <xsd:annotation>
