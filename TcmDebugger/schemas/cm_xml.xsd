                <xsd:element name="Data" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Read object properties</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="Title" type="tcm:Title"/>
