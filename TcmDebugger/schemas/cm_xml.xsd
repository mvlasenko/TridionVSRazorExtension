                        <xsd:element name="MinOccurs" type="xsd:nonNegativeInteger"/>
                        <xsd:element name="MaxOccurs" type="tcm:Occurrence"/>
                        <xsd:element name="AllowMultimediaLinks" type="xsd:boolean" default="false" minOccurs="0"/>
                        <xsd:element name="AllowedTargetSchemas">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TargetSchema" type="tcm:SimpleLink" minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="DefaultValue" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="ExtensionXml" type="tcm:CustomMultiXML" minOccurs="0"/>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="ExternalLinkField">
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element name="Name" type="xsd:NCName"/>
                        <xsd:element name="Description" type="tcm:NonEmptyString"/>
                        <xsd:element name="CustomURL" type="tcm:URIorEmpty" minOccurs="0"/>
                        <xsd:element name="MinOccurs" type="xsd:nonNegativeInteger"/>
                        <xsd:element name="MaxOccurs" type="tcm:Occurrence"/>
                        <xsd:element name="DefaultValue" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="ExtensionXml" type="tcm:CustomMultiXML" minOccurs="0"/>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="EmbeddedSchemaField">
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element name="Name" type="xsd:NCName"/>
                        <xsd:element name="Description" type="tcm:NonEmptyString"/>
                        <xsd:element name="CustomURL" type="tcm:URIorEmpty" minOccurs="0"/>
                        <xsd:element name="MinOccurs" type="xsd:nonNegativeInteger"/>
                        <xsd:element name="MaxOccurs" type="tcm:Occurrence"/>
                        <xsd:element name="EmbeddedSchema">
                            <xsd:complexType>
                                <xsd:complexContent>
                                    <xsd:extension base="tcm:SimpleLink">
                                        <xsd:sequence>
                                            <xsd:element name="Fields" type="tcm:TridionWebSchemaFields" minOccurs="0"/>
                                        </xsd:sequence>
                                    </xsd:extension>
                                </xsd:complexContent>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ExtensionXml" type="tcm:CustomMultiXML" minOccurs="0"/>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="KeywordField">
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element name="Name" type="xsd:NCName"/>
                        <xsd:element name="Description" type="tcm:NonEmptyString"/>
                        <xsd:element name="CustomURL" type="tcm:URIorEmpty" minOccurs="0"/>
                        <xsd:element name="MinOccurs" type="xsd:nonNegativeInteger"/>
                        <xsd:element name="MaxOccurs" type="tcm:Occurrence"/>
                        <xsd:element name="Category" type="tcm:SimpleLink"/>
                        <xsd:element name="List">
                            <xsd:complexType>
                                <xsd:attribute name="Type" type="tcm:ListType" use="required"/>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="Size" type="xsd:nonNegativeInteger"/>
                        <xsd:element name="DefaultValue" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="ExtensionXml" type="tcm:CustomMultiXML" minOccurs="0"/>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
        </xsd:choice>
    </xsd:complexType>
    <xsd:element name="TridionWebSchema">
        <xsd:annotation>
            <xsd:documentation>Fields representation for a Tridion Web Schema</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="tcm:Context" minOccurs="0"/>
                <xsd:element name="Info" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element ref="tcm:LocationInfo" minOccurs="0"/>
                            <xsd:element ref="tcm:BluePrintInfo" minOccurs="0"/>
                            <xsd:element name="VersionInfo" type="tcm:FullVersionInfo" minOccurs="0"/>
                            <xsd:element ref="tcm:LockInfo" minOccurs="0"/>
                            <xsd:element ref="tcm:PublishInfo" minOccurs="0"/>
                            <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                            <xsd:element name="DynamicVersionInfo" type="tcm:DynamicVersionInfo" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Data" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="Title" type="tcm:Title" minOccurs="0"/>
                            <xsd:element name="Description" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Note: Description cannot be empty, but this is not enforced on this level.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="Purpose" type="tcm:SchemaPurposes" minOccurs="0"/>
                            <xsd:element name="NamespaceURI" type="tcm:URIorEmpty" minOccurs="0"/>
                            <xsd:element name="RootElementName" type="tcm:NCNameOrEmpty" minOccurs="0"/>
                            <xsd:element name="Fields" type="tcm:TridionWebSchemaFields" minOccurs="0"/>
                            <xsd:element name="MetadataFields" type="tcm:TridionWebSchemaFields" minOccurs="0"/>
                            <xsd:element name="AllowedMultimediaTypes" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="AllowedMultimediaType" type="tcm:SimpleLink" minOccurs="0" maxOccurs="unbounded"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="ComponentProcess" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="BundleProcess" type="tcm:SimpleLink" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Schema" type="tcm:Schema_Type">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager Schema object (custom schema)</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="Schema_Type">
        <xsd:sequence>
            <xsd:element ref="tcm:Context" minOccurs="0"/>
            <xsd:element name="Info" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Read-only object info</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element ref="tcm:LocationInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:BluePrintInfo" minOccurs="0"/>
                        <xsd:element name="VersionInfo" type="tcm:FullVersionInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:LockInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:PublishInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                        <xsd:element name="DynamicVersionInfo" type="tcm:DynamicVersionInfo" minOccurs="0"/>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Data" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Read/write object properties</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element name="Title" type="tcm:Title" minOccurs="0"/>
                        <xsd:element name="Description" type="tcm:NonEmptyString" minOccurs="0"/>
                        <xsd:element name="Purpose" type="tcm:SchemaPurposes" minOccurs="0"/>
                        <xsd:element name="IsTridionWebSchema" type="xsd:boolean" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The value of this element is determined at BL-level. It is read-only on Kernel-level.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="NamespaceURI" type="tcm:URIorEmpty" minOccurs="0"/>
                        <xsd:element name="RootElementName" type="tcm:NCNameOrEmpty" minOccurs="0"/>
                        <xsd:element name="XSD" type="tcm:XSD" minOccurs="0"/>
                        <xsd:element name="AllowedMultimediaTypes" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="AllowedMultimediaType" type="tcm:SimpleLink" minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ComponentProcess" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="BundleProcess" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="DeleteBundleOnProcessFinished" type="xsd:boolean" minOccurs="0"/>
                        <xsd:element name="RegionDefinition" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="ComponentPresentationConstraints" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                                <xsd:element name="OccurenceConstraint">
                                                    <xsd:complexType>
                                                        <xsd:all>
                                                            <xsd:element name="MinOccurs" type="xsd:nonNegativeInteger"/>
                                                            <xsd:element name="MaxOccurs" type="tcm:Occurrence"/>
                                                        </xsd:all>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="TypeConstraint">
                                                    <xsd:complexType>
                                                        <xsd:choice maxOccurs="2">
                                                            <xsd:element name="BasedOnSchema" type="tcm:SimpleLink" minOccurs="0"/>
                                                            <xsd:element name="BasedOnComponentTemplate" type="tcm:SimpleLink" minOccurs="0"/>
                                                        </xsd:choice>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="LocationConstraint">
                                                    <xsd:complexType>
                                                        <xsd:all>
                                                            <xsd:element name="ComponentInFolder" type="tcm:SimpleLink"/>
                                                            <xsd:element name="AllowSubFolders" type="xsd:boolean"/>
                                                        </xsd:all>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="ClassificationConstraint">
                                                    <xsd:complexType>
                                                        <xsd:all>
                                                            <xsd:element name="ComponentClassifiedWith" type="tcm:SimpleLink"/>
                                                            <xsd:element name="AllowDescendantKeyword" type="xsd:boolean"/>
                                                        </xsd:all>
                                                    </xsd:complexType>
                                                </xsd:element>
                                            </xsd:choice>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="DefaultComponentPresentations" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="ComponentPresentation" type="tcm:ComponentPresentation_Type" minOccurs="0" maxOccurs="unbounded"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="NestedRegions" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="Region" minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:complexType>
                                                        <xsd:complexContent>
                                                            <xsd:extension base="tcm:SimpleLink">
                                                                <xsd:sequence>
                                                                    <xsd:element ref="tcm:Schema" minOccurs="0"/>
                                                                </xsd:sequence>
                                                                <xsd:attribute name="Name" type="xsd:NCName" use="required"/>
                                                            </xsd:extension>
                                                        </xsd:complexContent>
                                                    </xsd:complexType>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
    </xsd:complexType>
    <xsd:element name="Component" type="tcm:Component_Type">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager Component object</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="Component_Type">
        <xsd:sequence>
            <xsd:element ref="tcm:Context" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Information in regards to the context of the item in Tridion Content Manager.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Info" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Read-only object info</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element ref="tcm:LocationInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:BluePrintInfo" minOccurs="0"/>
                        <xsd:element name="VersionInfo" type="tcm:FullVersionInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:LockInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:PublishInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                        <xsd:element ref="tcm:WorkflowInfo" minOccurs="0"/>
                        <xsd:element name="DynamicVersionInfo" type="tcm:DynamicVersionInfo" minOccurs="0"/>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Data" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Read/write object properties</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element name="Title" type="tcm:Title" minOccurs="0"/>
                        <xsd:element name="Type" type="tcm:ComponentTypes" minOccurs="0"/>
                        <xsd:element name="Schema" minOccurs="0">
                            <xsd:complexType>
                                <xsd:complexContent>
                                    <xsd:extension base="tcm:SimpleLink">
                                        <xsd:attribute name="IsMandatory" type="xsd:boolean" use="optional"/>
                                    </xsd:extension>
                                </xsd:complexContent>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="Content" type="tcm:CustomXML" minOccurs="0"/>
                        <xsd:element name="Metadata" type="tcm:CustomXML" minOccurs="0"/>
                        <xsd:element name="ApprovalStatus" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="MultimediaType" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="IsBasedOnTridionWebSchema" type="xsd:boolean" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The value of this element is determined at BL-level. It is read-only on Kernel-level.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="MultimediaFilename" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="UploadedMultimediaFilename" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="MultimediaFileSize" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="IsExternalMultimediaFile" type="xsd:boolean" minOccurs="0"/>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
    </xsd:complexType>
    <xsd:element name="ComponentTemplate" type="tcm:ComponentTemplate_Type">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager ComponentTemplate object</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="ComponentTemplate_Type">
        <xsd:sequence>
            <xsd:element ref="tcm:Context" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Information in regards to the context of the item in Tridion Content Manager.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Info" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Read-only object info</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element ref="tcm:LocationInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:BluePrintInfo" minOccurs="0"/>
                        <xsd:element name="VersionInfo" type="tcm:FullVersionInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:LockInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:PublishInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                        <xsd:element ref="tcm:WorkflowInfo" minOccurs="0"/>
                        <xsd:element name="DynamicVersionInfo" type="tcm:DynamicVersionInfo" minOccurs="0"/>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Data" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Read/write object properties</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType mixed="false">
                    <xsd:all>
                        <xsd:element name="Title" type="tcm:Title" minOccurs="0"/>
                        <xsd:element name="Type" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="Priority" default="200" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:int">
                                    <xsd:enumeration value="0"/>
                                    <xsd:enumeration value="100"/>
                                    <xsd:enumeration value="200"/>
                                    <xsd:enumeration value="300"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="Content" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="PublisherScript" type="xsd:string" minOccurs="0"/>
                                    <xsd:element name="DynamicTemplate" type="xsd:string" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ApprovalStatus" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="RelatedSchemas" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="RelatedSchema" type="tcm:SimpleLink" minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="AllowOnPage" type="xsd:boolean" minOccurs="0"/>
                        <xsd:element name="IsRepositoryPublishable" type="xsd:boolean" minOccurs="0"/>
                        <xsd:element name="OutputFormat" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value="HTML"/>
                                    <xsd:enumeration value="HTML Fragment"/>
                                    <xsd:enumeration value="ASP VBScript"/>
                                    <xsd:enumeration value="ASP JScript"/>
                                    <xsd:enumeration value="JSP Scripting"/>
                                    <xsd:enumeration value="Plain Text"/>
                                    <xsd:enumeration value="XML Document"/>
                                    <xsd:enumeration value="XML Fragment"/>
                                    <xsd:enumeration value="ASCX WebControl"/>
                                    <xsd:enumeration value="REL"/>
                                    <xsd:enumeration value="Other"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="TrackingCategories" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TrackingCategory" type="tcm:SimpleLink" minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ParameterSchema" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="MetadataSchema" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="MultimediaFilename" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="MultimediaFileSize" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="IsExternalMultimediaFile" type="xsd:boolean" minOccurs="0"/>
                        <xsd:element name="UploadedMultimediaFilename" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="Metadata" type="tcm:CustomXML" minOccurs="0"/>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
    </xsd:complexType>
    <xsd:element name="TemplateBuildingBlock" type="tcm:TemplateBuildingBlock_Type">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager Template Building Block object</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="TemplateBuildingBlock_Type">
        <xsd:sequence>
            <xsd:element ref="tcm:Context" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Information in regards to the context of the item in Tridion Content Manager.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Info" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Read-only object info</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element ref="tcm:LocationInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:BluePrintInfo" minOccurs="0"/>
                        <xsd:element name="VersionInfo" type="tcm:FullVersionInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:LockInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:PublishInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                        <xsd:element ref="tcm:WorkflowInfo" minOccurs="0"/>
                        <xsd:element name="DynamicVersionInfo" type="tcm:DynamicVersionInfo" minOccurs="0"/>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Data" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Read/write object properties</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType mixed="false">
                    <xsd:all>
                        <xsd:element name="Title" type="tcm:Title" minOccurs="0"/>
                        <xsd:element name="Type" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="Content" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="PublisherScript" type="xsd:string" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ApprovalStatus" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="MultimediaFilename" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="UploadedMultimediaFilename" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="MultimediaFileSize" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="IsExternalMultimediaFile" type="xsd:boolean" minOccurs="0"/>
                        <xsd:element name="ParameterSchema" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="MetadataSchema" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="Metadata" type="tcm:CustomXML" minOccurs="0"/>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
    </xsd:complexType>
    <xsd:element name="Page" type="tcm:Page_Type">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager Page object</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="Page_Type">
        <xsd:sequence>
            <xsd:element ref="tcm:Context" minOccurs="0"/>
            <xsd:element name="Info" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Read-only object info</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element ref="tcm:LocationInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:BluePrintInfo" minOccurs="0"/>
                        <xsd:element name="VersionInfo" type="tcm:FullVersionInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:LockInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:PublishInfo" minOccurs="0"/>
                        <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                        <xsd:element ref="tcm:WorkflowInfo" minOccurs="0"/>
                        <xsd:element name="DynamicVersionInfo" type="tcm:DynamicVersionInfo" minOccurs="0"/>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Data" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Read/write object properties</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element name="Title" type="tcm:Title" minOccurs="0"/>
                        <xsd:element name="FileName" type="tcm:FileName" minOccurs="0"/>
                        <xsd:element name="PageTemplate" minOccurs="0">
                            <xsd:complexType>
                                <xsd:complexContent>
                                    <xsd:extension base="tcm:SimpleLink">
                                        <xsd:attribute name="IsInherited" type="xsd:boolean" use="optional" default="false"/>
                                    </xsd:extension>
                                </xsd:complexContent>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ComponentPresentations" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="ComponentPresentation" type="tcm:ComponentPresentation_Type" minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ApprovalStatus" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="MetadataSchema" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="Metadata" type="tcm:CustomXML" minOccurs="0"/>
                        <xsd:element name="RegionSchema" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="Regions" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="Region" type="tcm:Region_Type" minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
    </xsd:complexType>
    <xsd:complexType name="ComponentPresentation_Type">
        <xsd:sequence>
            <xsd:element name="Component" type="tcm:SimpleLink"/>
            <xsd:element name="ComponentTemplate" type="tcm:SimpleLink"/>
            <xsd:element name="Conditions" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Condition" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:all>
                                    <xsd:element name="TargetGroup" type="tcm:SimpleLink"/>
                                </xsd:all>
                                <xsd:attribute name="Negate" type="xsd:boolean" use="required"/>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="Region_Type">
        <xsd:sequence>
            <xsd:element name="RegionName" type="xsd:NCName" minOccurs="0"/>
            <xsd:element name="ComponentPresentations" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="ComponentPresentation" type="tcm:ComponentPresentation_Type" minOccurs="0" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Metadata" type="tcm:CustomXML" minOccurs="0"/>
            <xsd:element name="RegionSchema" type="tcm:SimpleLink" minOccurs="0"/>
            <xsd:element name="Regions" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Region" type="tcm:Region_Type" minOccurs="0" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="PageTemplate">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager PageTemplate object</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="tcm:Context" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Information in regards to the context of the item within Tridion Content Manager.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="Info" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Read-only object info</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element ref="tcm:LocationInfo" minOccurs="0"/>
                            <xsd:element ref="tcm:BluePrintInfo" minOccurs="0"/>
                            <xsd:element name="VersionInfo" type="tcm:FullVersionInfo" minOccurs="0"/>
                            <xsd:element ref="tcm:LockInfo" minOccurs="0"/>
                            <xsd:element ref="tcm:PublishInfo" minOccurs="0"/>
                            <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                            <xsd:element ref="tcm:WorkflowInfo" minOccurs="0"/>
                            <xsd:element name="DynamicVersionInfo" type="tcm:DynamicVersionInfo" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Data" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Read/write object properties</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType mixed="false">
                        <xsd:all>
                            <xsd:element name="Title" type="tcm:Title" minOccurs="0"/>
                            <xsd:element name="FileExtension" type="tcm:FileExtension" minOccurs="0"/>
                            <xsd:element name="Content" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:all>
                                        <xsd:element name="PublisherScript" type="xsd:string" minOccurs="0"/>
                                    </xsd:all>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="ApprovalStatus" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="Type" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="MultimediaFilename" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="UploadedMultimediaFilename" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="MultimediaFileSize" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="IsExternalMultimediaFile" type="xsd:boolean" minOccurs="0"/>
                            <xsd:element name="ParameterSchema" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="MetadataSchema" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="Metadata" type="tcm:CustomXML" minOccurs="0"/>
                            <xsd:element name="PageSchema" type="tcm:SimpleLink" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:simpleType name="TargetGroupConditionType">
        <xsd:annotation>
            <xsd:documentation>Used in TargetGroup</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Include"/>
            <xsd:enumeration value="Exclude"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="TargetGroupNumericalConditionOperator">
        <xsd:annotation>
            <xsd:documentation>Used in TargetGroup</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="="/>
            <xsd:enumeration value="&lt;"/>
            <xsd:enumeration value="&gt;"/>
            <xsd:enumeration value="&lt;&gt;"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="TargetGroupAllConditionOperator">
        <xsd:annotation>
            <xsd:documentation>Used in TargetGroup</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="="/>
            <xsd:enumeration value="&lt;"/>
            <xsd:enumeration value="&gt;"/>
            <xsd:enumeration value="&lt;&gt;"/>
            <xsd:enumeration value="equals"/>
            <xsd:enumeration value="contains"/>
            <xsd:enumeration value="startswith"/>
            <xsd:enumeration value="endswith"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:element name="TargetGroup">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager TargetGroup object</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="tcm:Context" minOccurs="0"/>
                <xsd:element name="Info" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element ref="tcm:LocationInfo" minOccurs="0"/>
                            <xsd:element ref="tcm:BluePrintInfo" minOccurs="0"/>
                            <xsd:element name="VersionInfo" type="tcm:BasicVersionInfo" minOccurs="0"/>
                            <xsd:element ref="tcm:PublishInfo" minOccurs="0"/>
                            <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Data" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="Title" type="tcm:Title" minOccurs="0"/>
                            <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Conditions" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:choice minOccurs="0" maxOccurs="unbounded">
                                        <xsd:element name="TargetGroupCondition">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="TargetGroup" type="tcm:SimpleLink"/>
                                                </xsd:sequence>
                                                <xsd:attribute name="Type" type="tcm:TargetGroupConditionType" use="required"/>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="CustomerCharacteristicCondition">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="Title" type="tcm:Title"/>
                                                    <xsd:element name="Value" type="xsd:string"/>
                                                </xsd:sequence>
                                                <xsd:attribute name="Type" type="tcm:TargetGroupConditionType" use="required"/>
                                                <xsd:attribute name="Operator" type="tcm:TargetGroupAllConditionOperator" use="required"/>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="KeywordCondition">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="Keyword" type="tcm:SimpleLink"/>
                                                    <xsd:element name="Value" type="xsd:string"/>
                                                </xsd:sequence>
                                                <xsd:attribute name="Type" type="tcm:TargetGroupConditionType" use="required"/>
                                                <xsd:attribute name="Operator" type="tcm:TargetGroupNumericalConditionOperator" use="required"/>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:choice>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="MultimediaType">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager MultimediaType object</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Info" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Data" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="Title" type="tcm:Title" minOccurs="0"/>
                            <xsd:element name="MIMEType" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="FileExtensions" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="FileExtension" type="tcm:FileExtension" maxOccurs="unbounded"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PublicationTarget" type="tcm:PublicationTarget_Type">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager PublicationTarget object</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="PublicationTarget_Type">
        <xsd:sequence>
            <xsd:element name="Info" minOccurs="0">
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Data" minOccurs="0">
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element name="Destinations" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="Destination" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="Title" type="tcm:Title"/>
                                                <xsd:element name="Protocol" type="tcm:SimpleLink"/>
                                                <xsd:element name="ProtocolFields" type="tcm:CustomXML"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="Publications" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="Publication" type="tcm:SimpleLink" minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="TargetTypes" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TargetType" type="tcm:SimpleLink" minOccurs="0" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="Title" type="tcm:Title" minOccurs="0"/>
                        <xsd:element name="Description" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:minLength value="1"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="MinApprovalStatus" type="tcm:SimpleLink" minOccurs="0"/>
                        <xsd:element name="ScriptingLanguage" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value="ASP/JavaScript"/>
                                    <xsd:enumeration value="ASP/VBScript"/>
                                    <xsd:enumeration value="JSP"/>
                                    <xsd:enumeration value="None"/>
                                    <xsd:enumeration value="TCDL"/>
                                    <xsd:enumeration value="ASP.NET"/>
                                    <xsd:enumeration value="REL"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="DefaultCodePage" type="xsd:integer" minOccurs="0"/>
                        <xsd:element name="Tracked" type="xsd:boolean" minOccurs="0"/>
                        <xsd:element name="Priority" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value="2"/>
                                    <xsd:enumeration value="4"/>
                                    <xsd:enumeration value="6"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
    </xsd:complexType>
    <xsd:element name="TargetType">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager TargetType object</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Info" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Data" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="Title" type="tcm:Title" minOccurs="0"/>
                            <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element ref="tcm:Permissions" minOccurs="0"/>
            </xsd:sequence>
            <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ApprovalStatus">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager ApprovalStatus object</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Data" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="Title" type="tcm:Title" minOccurs="0"/>
                            <xsd:element name="Position" type="xsd:integer" minOccurs="0"/>
                            <xsd:element name="Deleted" type="xsd:boolean" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ScheduleItem">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager ScheduleItem object</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Data" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="Publication" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="PublicationTarget" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="Time" type="xsd:dateTime" minOccurs="0"/>
                            <xsd:element ref="tcm:ItemLink" minOccurs="0"/>
                            <xsd:element name="Publisher" type="tcm:SimpleLink" minOccurs="0"/>
                            <xsd:element name="Instruction" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:attribute name="Title" type="xsd:string" use="required"/>
                                    <xsd:attribute name="Type" type="xsd:int" use="required"/>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Content" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="PublisherTransaction" type="tcm:SimpleLink"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Group">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager Group object</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="tcm:Context" minOccurs="0"/>
                <xsd:element name="Info" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Data" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="Name" type="tcm:NonEmptyString" minOccurs="0"/>
                            <xsd:element name="Description" type="tcm:NonEmptyString" minOccurs="0"/>
                            <xsd:element ref="tcm:GroupMemberships" minOccurs="0"/>
                            <xsd:element name="DirectoryGroupMappings" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="DirectoryGroupMapping" minOccurs="0" maxOccurs="unbounded">
                                            <xsd:complexType>
                                                <xsd:all>
                                                    <xsd:element name="SearchDN" type="xsd:string"/>
                                                    <xsd:element name="SearchType" type="tcm:directorySearchType"/>
                                                    <xsd:element name="Description" type="xsd:string"/>
                                                    <xsd:element ref="tcm:DirectoryService"/>
                                                </xsd:all>
                                                <xsd:attribute name="ID" type="xsd:anyURI" use="required"/>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                    <xsd:attribute name="Enabled" type="xsd:boolean" use="optional" default="true">
                                        <xsd:annotation>
                                            <xsd:documentation>This attribute is the functional replacement for TDSE.DirectoryServicesEnabled. If it is set to false, the directory service mappings should not be displayed in the user interface</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="UsageConstraints" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="Publication" minOccurs="0" maxOccurs="unbounded">
                                            <xsd:complexType>
                                                <xsd:complexContent>
                                                    <xsd:extension base="tcm:SimpleLink">
                                                        <xsd:attribute name="IsEditable" type="xsd:boolean" use="optional"/>
                                                    </xsd:extension>
                                                </xsd:complexContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
            <xsd:attribute name="IsPredefined" type="xsd:boolean" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="User">
        <xsd:annotation>
            <xsd:documentation>Tridion Content Manager User object</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="tcm:Context" minOccurs="0"/>
                <xsd:element name="Info" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element ref="tcm:AllowedActions" minOccurs="0"/>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Data" minOccurs="0">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element name="Name" type="tcm:NonEmptyString" minOccurs="0"/>
                            <xsd:element name="Description" type="tcm:NonEmptyString" minOccurs="0"/>
                            <xsd:element name="Enabled" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:simpleContent>
                                        <xsd:extension base="xsd:boolean">
                                            <xsd:attribute name="IsEditable" type="xsd:boolean" use="optional" default="true"/>
                                        </xsd:extension>
                                    </xsd:simpleContent>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="LanguageID" type="xsd:int" nillable="false" minOccurs="0"/>
                            <xsd:element name="LocaleID" type="xsd:int" nillable="false" minOccurs="0"/>
                            <xsd:element ref="tcm:GroupMemberships" minOccurs="0"/>
                            <xsd:element name="Privileges" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:simpleContent>
                                        <xsd:extension base="xsd:int">
                                            <xsd:attribute name="IsEditable" type="xsd:boolean" use="optional" default="true"/>
                                        </xsd:extension>
                                    </xsd:simpleContent>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attributeGroup ref="tcm:ObjectAttrs"/>
            <xsd:attribute name="IsPredefined" type="xsd:boolean" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Container">
        <xsd:annotation>
            <xsd:documentation>Element allowing a mix of any system schema defined global element</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:any namespace="http://www.tridion.com/ContentManager/5.0" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemContainer">
        <xsd:annotation>
            <xsd:documentation>Document element for multi-item documents</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element ref="tcm:Publication"/>
                <xsd:element ref="tcm:Folder"/>
                <xsd:element ref="tcm:StructureGroup"/>
                <xsd:element ref="tcm:Schema"/>
                <xsd:element ref="tcm:TridionWebSchema"/>
                <xsd:element ref="tcm:Component"/>
                <xsd:element ref="tcm:ComponentTemplate"/>
                <xsd:element ref="tcm:Page"/>
                <xsd:element ref="tcm:PageTemplate"/>
                <xsd:element ref="tcm:TargetGroup"/>
                <xsd:element ref="tcm:MultimediaType"/>
                <xsd:element ref="tcm:PublicationTarget"/>
                <xsd:element ref="tcm:ApprovalStatus"/>
                <xsd:element ref="tcm:ScheduleItem"/>
                <xsd:element ref="tcm:User"/>
                <xsd:element ref="tcm:Group"/>
            </xsd:choice>
        </xsd:complexType>
    </xsd:element>
    <xsd:attributeGroup name="CommonListItemAttrs">
        <xsd:annotation>
            <xsd:documentation>Common attributes for list items (see below)</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="ID" type="xsd:anyURI" use="required"/>
        <xsd:attribute name="Title" type="xsd:string" use="optional"/>
        <xsd:attribute name="Icon" type="xsd:string" use="optional"/>
    </xsd:attributeGroup>
    <xsd:attributeGroup name="ExtendedListItemAttrs">
        <xsd:annotation>
            <xsd:documentation>Extended attributes for list items (returned if XMLListExtended is used)</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="IsShared" type="xsd:boolean" use="optional"/>
        <xsd:attribute name="IsLocalized" type="xsd:boolean" use="optional"/>
        <xsd:attribute name="IsPublished" type="xsd:boolean" use="optional"/>
        <xsd:attribute name="Lock" type="xsd:int" use="optional"/>
        <xsd:attribute name="Trustee" type="xsd:anyURI" use="optional"/>
        <xsd:attribute name="SubType" type="xsd:int" use="optional"/>
        <xsd:attribute name="FileExt" type="xsd:string" use="optional"/>
    </xsd:attributeGroup>
    <xsd:attributeGroup name="AllowedActionsListItemAttrs">
        <xsd:annotation>
            <xsd:documentation>Allowed actions attributes for list items (returned if XMLListAllowedActions is used)</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="Allow" type="xsd:int" use="optional"/>
        <xsd:attribute name="Deny" type="xsd:int" use="optional"/>
        <xsd:attribute name="Managed" type="xsd:int" use="optional"/>
    </xsd:attributeGroup>
    <xsd:element name="ListItems">
        <xsd:annotation>
            <xsd:documentation>Compact representation of org. item content list</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
